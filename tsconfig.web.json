{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "compilerOptions": {
    "composite": true,
    "strict": true,
    "strictNullChecks": false,
    "strictPropertyInitialization": false,

    "jsx": "react-jsx",
    "types": ["vite/client"]
  },
  "include": [
    "src/renderer/**/*",
    "src/preload/index.d.ts",
    "src/common/**/*",
    "src/emu/**/*",
    "src/script-packages/**/*"
  ]
}
