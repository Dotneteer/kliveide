; ---------------------------
;
;

<a name=3D"L0CBA"></a>;; <b>LINE-SCAN</b>
L0CBA:  LD      (IY+$01),$01    ; sv FLAGS
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0A73">L0A73</a>           ; routine E-LINE-NO

<a name=3D"L0CC1"></a>;; <b>LINE-RUN</b>
L0CC1:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L14BC">L14BC</a>           ; routine SET-MIN
        LD      HL,$4000        ; sv ERR_NR
        LD      (HL),$FF        ;
        LD      HL,$402D        ; sv FLAGX
        BIT     5,(HL)          ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0CDE">L0CDE</a>         ; to LINE-NULL

        CP      $E3             ; 'STOP' ?
        LD      A,(HL)          ;
        JP      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0D6F">L0D6F</a>        ; to INPUT-REP

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0DA6">L0DA6</a>           ; routine SYNTAX-Z
        RET     Z               ;


        RST     08H             ; ERROR-1
        DEFB    $0C             ; Error Report: BREAK - CONT repeats


; --------------------------
; THE <b><font color=3D"#333388">'STOP'</font></b> COMMAND ROUTINE
; --------------------------
;
;

<a name=3D"L0CDC"></a>;; <b>STOP</b>
L0CDC:  RST     08H             ; ERROR-1
        DEFB    $08             ; Error Report: STOP statement

; ---

; the interpretation of a line continues with a check for just spaces
; followed by a carriage return.
; The IF command also branches here with a true value to execute the
; statement after the THEN but the statement can be null so
; 10 IF 1 =3D 1 THEN
; passes syntax (on all ZX computers).

<a name=3D"L0CDE"></a>;; <b>LINE-NULL</b>
L0CDE:  RST     18H             ; GET-CHAR
        LD      B,$00           ; prepare to index - early.
        CP      $76             ; compare to NEWLINE.
        RET     Z               ; return if so.

        LD      C,A             ; transfer character to C.

        RST     20H             ; NEXT-CHAR advances.
        LD      A,C             ; character to A
        SUB     $E1             ; subtract 'LPRINT' - lowest command.
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0D26">L0D26</a>         ; forward if less to REPORT-C2

        LD      C,A             ; reduced token to C
        LD      HL,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0C29">L0C29</a>        ; set HL to address of offset table.
        ADD     HL,BC           ; index into offset table.
        LD      C,(HL)          ; fetch offset
        ADD     HL,BC           ; index into parameter table.
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0CF7">L0CF7</a>           ; to GET-PARAM

; ---

<a name=3D"L0CF4"></a>;; <b>SCAN-LOOP</b>
L0CF4:  LD      HL,($4030)      ; sv T_ADDR_lo

; -&gt; Entry Point to Scanning Loop

<a name=3D"L0CF7"></a>;; <b>GET-PARAM</b>
L0CF7:  LD      A,(HL)          ;
        INC     HL              ;
        LD      ($4030),HL      ; sv T_ADDR_lo

        LD      BC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0CF4">L0CF4</a>        ; Address: SCAN-LOOP
        PUSH    BC              ; is pushed on machine stack.

        LD      C,A             ;
        CP      $0B             ;
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0D10">L0D10</a>        ; to SEPARATOR

        LD      HL,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0D16">L0D16</a>        ; class-tbl - the address of the class table=
.
        LD      B,$00           ;
        ADD     HL,BC           ;
        LD      C,(HL)          ;
        ADD     HL,BC           ;
        PUSH    HL              ;

        RST     18H             ; GET-CHAR
        RET                     ; indirect jump to class routine and
                                ; by subsequent RET to SCAN-LOOP.

; -----------------------
; THE <b><font color=3D"#333388">'SEPARATOR'</font></b> ROUTINE
; -----------------------

<a name=3D"L0D10"></a>;; <b>SEPARATOR</b>
L0D10:  RST     18H             ; GET-CHAR
        CP      C               ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0D26">L0D26</a>        ; to REPORT-C2
                                ; 'Nonsense in BASIC'

        RST     20H             ; NEXT-CHAR
        RET                     ; return


