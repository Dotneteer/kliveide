        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0D3A">L0D3A</a>        ; to CLASS-END

        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0D22">L0D22</a>           ; to CHECK-2

; ---

<a name=3D"L0D85"></a>;; <b>CLASS-4</b>
L0D85:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L111C">L111C</a>           ; routine LOOK-VARS
        PUSH    AF              ;
        LD      A,C             ;
        OR      $9F             ;
        INC     A               ;
        JR       NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/z=
x81.htm#L0D9A">L0D9A</a>       ; to REPORT-C

        POP     AF              ;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0D3F">L0D3F</a>           ; to CLASS-4-2

; ---

<a name=3D"L0D92"></a>;; <b>CLASS-6</b>
L0D92:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0F55">L0F55</a>           ; routine SCANNING
        BIT     6,(IY+$01)      ; sv FLAGS  - Numeric or string result?
        RET     NZ              ;


<a name=3D"L0D9A"></a>;; <b>REPORT-C</b>
L0D9A:  RST     08H             ; ERROR-1
        DEFB    $0B             ; Error Report: Nonsense in BASIC

; --------------------------------
; THE <b><font color=3D"#333388">'NUMBER TO STACK'</font></b> SUBROUTINE
; --------------------------------
;
;

<a name=3D"L0D9C"></a>;; <b>NO-TO-STK</b>
L0D9C:  JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0D92">L0D92</a>        ; back to CLASS-6 with a non-zero number.

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0DA6">L0DA6</a>           ; routine SYNTAX-Z
        RET     Z               ; return if checking syntax.

; in runtime a zero default is placed on the calculator stack.

        RST     28H             ;; FP-CALC
        DEFB    $A0             ;;stk-zero
        DEFB    $34             ;;end-calc

        RET                     ; return.

; -------------------------
; THE <b><font color=3D"#333388">'SYNTAX-Z'</font></b> SUBROUTINE
; -------------------------
; This routine returns with zero flag set if checking syntax.
; Calling this routine uses three instruction bytes compared to four if the
; bit test is implemented inline.

<a name=3D"L0DA6"></a>;; <b>SYNTAX-Z</b>
L0DA6:  BIT     7,(IY+$01)      ; test FLAGS  - checking syntax only?
        RET                     ; return.

; ------------------------
; THE <b><font color=3D"#333388">'IF'</font></b> COMMAND ROUTINE
; ------------------------
; In runtime, the class routines have evaluated the test expression and
; the result, true or false, is on the stack.

<a name=3D"L0DAB"></a>;; <b>IF</b>
L0DAB:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0DA6">L0DA6</a>           ; routine SYNTAX-Z
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0DB6">L0DB6</a>         ; forward if checking syntax to IF-END

; else delete the Boolean value on the calculator stack.

        RST     28H             ;; FP-CALC
        DEFB    $02             ;;delete
        DEFB    $34             ;;end-calc

; register DE points to exponent of floating point value.

        LD      A,(DE)          ; fetch exponent.
        AND     A               ; test for zero - FALSE.
        RET     Z               ; return if so.

<a name=3D"L0DB6"></a>;; <b>IF-END</b>
L0DB6:  JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0CDE">L0CDE</a>           ; jump back to LINE-NULL

; -------------------------
; THE <b><font color=3D"#333388">'FOR'</font></b> COMMAND ROUTINE
; -------------------------
;
;

<a name=3D"L0DB9"></a>;; <b>FOR</b>
L0DB9:  CP      $E0             ; is current character 'STEP' ?
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0DC6">L0DC6</a>        ; forward if not to F-USE-ONE


        RST     20H             ; NEXT-CHAR
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0D92">L0D92</a>           ; routine CLASS-6 stacks the number
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0D1D">L0D1D</a>           ; routine CHECK-END
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0DCC">L0DCC</a>           ; forward to F-REORDER

; ---

<a name=3D"L0DC6"></a>;; <b>F-USE-ONE</b>
L0DC6:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0D1D">L0D1D</a>           ; routine CHECK-END

        RST     28H             ;; FP-CALC
        DEFB    $A1             ;;stk-one
        DEFB    $34             ;;end-calc



<a name=3D"L0DCC"></a>;; <b>F-REORDER</b>
L0DCC:  RST     28H             ;; FP-CALC      v, l, s.
        DEFB    $C0             ;;st-mem-0      v, l, s.
        DEFB    $02             ;;delete        v, l.
        DEFB    $01             ;;exchange      l, v.
        DEFB    $E0             ;;get-mem-0     l, v, s.
        DEFB    $01             ;;exchange      l, s, v.
        DEFB    $34             ;;end-calc      l, s, v.

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L1321">L1321</a>           ; routine LET

        LD      ($401F),HL      ; set MEM to address variable.
        DEC     HL              ; point to letter.
        LD      A,(HL)          ;
        SET     7,(HL)          ;
        LD      BC,$0006        ;
        ADD     HL,BC           ;
        RLCA                    ;
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0DEA">L0DEA</a>         ; to F-LMT-STP

        SLA     C               ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L099E">L099E</a>           ; routine MAKE-ROOM
        INC     HL              ;

<a name=3D"L0DEA"></a>;; <b>F-LMT-STP</b>
L0DEA:  PUSH    HL              ;

        RST     28H             ;; FP-CALC
        DEFB    $02             ;;delete
        DEFB    $02             ;;delete
        DEFB    $34             ;;end-calc

        POP     HL              ;
        EX      DE,HL           ;

        LD      C,$0A           ; ten bytes to be moved.
        LDIR                    ; copy bytes

        LD      HL,($4007)      ; set HL to system variable PPC current lin=
e.
        EX      DE,HL           ; transfer to DE, variable pointer to HL.
        INC     DE              ; loop start will be this line + 1 at least=
.
        LD      (HL),E          ;
        INC     HL              ;
        LD      (HL),D          ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0E5A">L0E5A</a>           ; routine NEXT-LOOP considers an initial pas=
s.
        RET     NC              ; return if possible.

; else program continues from point following matching NEXT.

        BIT     7,(IY+$08)      ; test PPC_hi
        RET     NZ              ; return if over 32767 ???

        LD      B,(IY+$2E)      ; fetch variable name from STRLEN_lo
        RES     6,B             ; make a true letter.
        LD      HL,($4029)      ; set HL from NXTLIN

; now enter a loop to look for matching next.

<a name=3D"L0E0E"></a>;; <b>NXTLIN-NO</b>
L0E0E:  LD      A,(HL)          ; fetch high byte of line number.
        AND     $C0             ; mask off low bits $3F
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0E2A">L0E2A</a>        ; forward at end of program to FOR-END

        PUSH    BC              ; save letter
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L09F2">L09F2</a>           ; routine NEXT-ONE finds next line.
        POP     BC              ; restore letter

        INC     HL              ; step past low byte
        INC     HL              ; past the
        INC     HL              ; line length.
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L004C">L004C</a>           ; routine TEMP-PTR1 sets CH_ADD

        RST     18H             ; GET-CHAR
        CP      $F3             ; compare to 'NEXT'.
        EX      DE,HL           ; next line to HL.
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0E0E">L0E0E</a>        ; back with no match to NXTLIN-NO

;

        EX      DE,HL           ; restore pointer.

        RST     20H             ; NEXT-CHAR advances and gets letter in A.
        EX      DE,HL           ; save pointer
        CP      B               ; compare to variable name.
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0E0E">L0E0E</a>        ; back with mismatch to NXTLIN-NO

<a name=3D"L0E2A"></a>;; <b>FOR-END</b>
L0E2A:  LD      ($4029),HL      ; update system variable NXTLIN
        RET                     ; return.

; --------------------------
; THE <b><font color=3D"#333388">'NEXT'</font></b> COMMAND ROUTINE
; --------------------------
;
;

<a name=3D"L0E2E"></a>;; <b>NEXT</b>
L0E2E:  BIT     1,(IY+$2D)      ; sv FLAGX
        JP      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0D4B">L0D4B</a>        ; to REPORT-2

        LD      HL,($4012)      ; DEST
        BIT     7,(HL)          ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0E58">L0E58</a>         ; to REPORT-1

        INC     HL              ;
        LD      ($401F),HL      ; sv MEM_lo

        RST     28H             ;; FP-CALC
        DEFB    $E0             ;;get-mem-0
        DEFB    $E2             ;;get-mem-2
        DEFB    $0F             ;;addition
        DEFB    $C0             ;;st-mem-0
        DEFB    $02             ;;delete
        DEFB    $34             ;;end-calc

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0E5A">L0E5A</a>           ; routine NEXT-LOOP
        RET     C               ;

        LD      HL,($401F)      ; sv MEM_lo
        LD      DE,$000F        ;
        ADD     HL,DE           ;
        LD      E,(HL)          ;
        INC     HL              ;
        LD      D,(HL)          ;
        EX      DE,HL           ;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0E86">L0E86</a>           ; to GOTO-2

; ---


<a name=3D"L0E58"></a>;; <b>REPORT-1</b>
L0E58:  RST     08H             ; ERROR-1
        DEFB    $00             ; Error Report: NEXT without FOR


; --------------------------
; THE <b><font color=3D"#333388">'NEXT-LOOP'</font></b> SUBROUTINE
; --------------------------
;
;

<a name=3D"L0E5A"></a>;; <b>NEXT-LOOP</b>
L0E5A:  RST     28H             ;; FP-CALC
        DEFB    $E1             ;;get-mem-1
        DEFB    $E0             ;;get-mem-0
        DEFB    $E2             ;;get-mem-2
        DEFB    $32             ;;less-0
        DEFB    $00             ;;jump-true
        DEFB    $02             ;;to <a href=3D"https://www.tablix.org/~avi=
an/spectrum/rom/zx81.htm#L0E62">L0E62</a>, LMT-V-VAL

        DEFB    $01             ;;exchange

<a name=3D"L0E62"></a>;; <b>LMT-V-VAL</b>
L0E62:  DEFB    $03             ;;subtract
        DEFB    $33             ;;greater-0
        DEFB    $00             ;;jump-true
        DEFB    $04             ;;to <a href=3D"https://www.tablix.org/~avi=
an/spectrum/rom/zx81.htm#L0E69">L0E69</a>, IMPOSS

        DEFB    $34             ;;end-calc

        AND     A               ; clear carry flag
        RET                     ; return.

; ---


<a name=3D"L0E69"></a>;; <b>IMPOSS</b>
L0E69:  DEFB    $34             ;;end-calc

        SCF                     ; set carry flag
        RET                     ; return.

; --------------------------
; THE <b><font color=3D"#333388">'RAND'</font></b> COMMAND ROUTINE
; --------------------------
; The keyword was <b><font color=3D"#333388">'RANDOMISE'</font></b> on the =
ZX80, is 'RAND' here on the ZX81 and
; becomes 'RANDOMIZE' on the ZX Spectrum.
; In all invocations the procedure is the same - to set the SEED system var=
iable
; with a supplied integer value or to use a time-based value if no number, =
or
; zero, is supplied.

<a name=3D"L0E6C"></a>;; <b>RAND</b>
L0E6C:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0EA7">L0EA7</a>           ; routine FIND-INT
        LD      A,B             ; test value
        OR      C               ; for zero
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0E77">L0E77</a>        ; forward if not zero to SET-SEED

        LD      BC,($4034)      ; fetch value of FRAMES system variable.

<a name=3D"L0E77"></a>;; <b>SET-SEED</b>
L0E77:  LD       ($4032),BC     ; update the SEED system variable.
        RET                     ; return.

; --------------------------
; THE <b><font color=3D"#333388">'CONT'</font></b> COMMAND ROUTINE
; --------------------------
; Another abbreviated command. ROM space was really tight.
; CONTINUE at the line number that was set when break was pressed.
; Sometimes the current line, sometimes the next line.

<a name=3D"L0E7C"></a>;; <b>CONT</b>
L0E7C:  LD      HL,($402B)      ; set HL from system variable OLDPPC
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0E86">L0E86</a>           ; forward to GOTO-2

; --------------------------
; THE <b><font color=3D"#333388">'GOTO'</font></b> COMMAND ROUTINE
; --------------------------
; This token also suffered from the shortage of room and there is no space
; getween GO and TO as there is on the ZX80 and ZX Spectrum. The same also=
=20
; applies to the GOSUB keyword.

<a name=3D"L0E81"></a>;; <b>GOTO</b>
L0E81:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0EA7">L0EA7</a>           ; routine FIND-INT
        LD      H,B             ;
        LD      L,C             ;

<a name=3D"L0E86"></a>;; <b>GOTO-2</b>
L0E86:  LD      A,H             ;
        CP      $F0             ;
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0EAD">L0EAD</a>        ; to REPORT-B

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L09D8">L09D8</a>           ; routine LINE-ADDR
        LD      ($4029),HL      ; sv NXTLIN_lo
        RET                     ;

; --------------------------
; THE <b><font color=3D"#333388">'POKE'</font></b> COMMAND ROUTINE
; --------------------------
;
;

<a name=3D"L0E92"></a>;; <b>POKE</b>
L0E92:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L15CD">L15CD</a>           ; routine FP-TO-A
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0EAD">L0EAD</a>         ; forward, with overflow, to REPORT-B

        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0E9B">L0E9B</a>         ; forward, if positive, to POKE-SAVE

        NEG                     ; negate

<a name=3D"L0E9B"></a>;; <b>POKE-SAVE</b>
L0E9B:  PUSH    AF              ; preserve value.
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0EA7">L0EA7</a>           ; routine FIND-INT gets address in BC
                                ; invoking the error routine with overflow
                                ; or a negative number.
        POP     AF              ; restore value.

; <font color=3D"#9900FF">Note.</font> the next two instructions are legacy=
 code from the ZX80 and
; inappropriate here.

        BIT     7,(IY+$00)      ; test ERR_NR - is it still $FF ?
        RET     Z               ; return with error.

        LD      (BC),A          ; update the address contents.
        RET                     ; return.

; -----------------------------
; THE <b><font color=3D"#333388">'FIND INTEGER'</font></b> SUBROUTINE
; -----------------------------
;
;

<a name=3D"L0EA7"></a>;; <b>FIND-INT</b>
L0EA7:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L158A">L158A</a>           ; routine FP-TO-BC
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0EAD">L0EAD</a>         ; forward with overflow to REPORT-B

        RET     Z               ; return if positive (0-65535).


<a name=3D"L0EAD"></a>;; <b>REPORT-B</b>
L0EAD:  RST     08H             ; ERROR-1
        DEFB    $0A             ; Error Report: Integer out of range

; -------------------------
; THE <b><font color=3D"#333388">'RUN'</font></b> COMMAND ROUTINE
; -------------------------
;
;

<a name=3D"L0EAF"></a>;; <b>RUN</b>
L0EAF:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0E81">L0E81</a>           ; routine GOTO
        JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L149A">L149A</a>           ; to CLEAR

; ---------------------------
; THE <b><font color=3D"#333388">'GOSUB'</font></b> COMMAND ROUTINE
; ---------------------------
;
;

<a name=3D"L0EB5"></a>;; <b>GOSUB</b>
L0EB5:  LD      HL,($4007)      ; sv PPC_lo
        INC     HL              ;
        EX      (SP),HL         ;
        PUSH    HL              ;
        LD      ($4002),SP      ; set the error stack pointer - ERR_SP
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0E81">L0E81</a>           ; routine GOTO
        LD      BC,$0006        ;

; --------------------------
; THE <b><font color=3D"#333388">'TEST ROOM'</font></b> SUBROUTINE
; --------------------------
;
;

<a name=3D"L0EC5"></a>;; <b>TEST-ROOM</b>
L0EC5:  LD      HL,($401C)      ; sv STKEND_lo
        ADD     HL,BC           ;
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0ED3">L0ED3</a>         ; to REPORT-4

        EX      DE,HL           ;
        LD      HL,$0024        ;
        ADD     HL,DE           ;
        SBC     HL,SP           ;
        RET     C               ;

<a name=3D"L0ED3"></a>;; <b>REPORT-4</b>
L0ED3:  LD      L,$03           ;
        JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0058">L0058</a>           ; to ERROR-3

; ----------------------------
; THE <b><font color=3D"#333388">'RETURN'</font></b> COMMAND ROUTINE
; ----------------------------
;
;

<a name=3D"L0ED8"></a>;; <b>RETURN</b>
L0ED8:  POP     HL              ;
        EX      (SP),HL         ;
        LD      A,H             ;
        CP      $3E             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0EE5">L0EE5</a>         ; to REPORT-7

        LD      ($4002),SP      ; sv ERR_SP_lo
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0E86">L0E86</a>           ; back to GOTO-2

; ---

<a name=3D"L0EE5"></a>;; <b>REPORT-7</b>
L0EE5:  EX      (SP),HL         ;
        PUSH    HL              ;

        RST     08H             ; ERROR-1
        DEFB    $06             ; Error Report: RETURN without GOSUB

; ---------------------------
; THE <b><font color=3D"#333388">'INPUT'</font></b> COMMAND ROUTINE
; ---------------------------
;
;

<a name=3D"L0EE9"></a>;; <b>INPUT</b>
L0EE9:  BIT     7,(IY+$08)      ; sv PPC_hi
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0F21">L0F21</a>        ; to REPORT-8

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L14A3">L14A3</a>           ; routine X-TEMP
        LD      HL,$402D        ; sv FLAGX
        SET     5,(HL)          ;
        RES     6,(HL)          ;
        LD      A,($4001)       ; sv FLAGS
        AND     $40             ;
        LD      BC,$0002        ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0F05">L0F05</a>        ; to PROMPT

        LD      C,$04           ;

<a name=3D"L0F05"></a>;; <b>PROMPT</b>
L0F05:  OR      (HL)            ;
        LD      (HL),A          ;

        RST     30H             ; BC-SPACES
        LD      (HL),$76        ;
        LD      A,C             ;
        RRCA                    ;
        RRCA                    ;
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0F14">L0F14</a>         ; to ENTER-CUR

        LD      A,$0B           ;
        LD      (DE),A          ;
        DEC     HL              ;
        LD      (HL),A          ;

<a name=3D"L0F14"></a>;; <b>ENTER-CUR</b>
L0F14:  DEC     HL              ;
        LD      (HL),$7F        ;
        LD      HL,($4039)      ; sv S_POSN_x
        LD      ($4030),HL      ; sv T_ADDR_lo
        POP     HL              ;
        JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0472">L0472</a>           ; to LOWER

; ---

<a name=3D"L0F21"></a>;; <b>REPORT-8</b>
L0F21:  RST     08H             ; ERROR-1
        DEFB    $07             ; Error Report: End of file

; ---------------------------
; THE <b><font color=3D"#333388">'PAUSE'</font></b> COMMAND ROUTINE
; ---------------------------
;
;

<a name=3D"L0F23"></a>;; <b>FAST</b>
L0F23:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L02E7">L02E7</a>           ; routine SET-FAST
        RES     6,(IY+$3B)      ; sv CDFLAG
        RET                     ; return.

; --------------------------
; THE <b><font color=3D"#333388">'SLOW'</font></b> COMMAND ROUTINE
; --------------------------
;
;

<a name=3D"L0F2B"></a>;; <b>SLOW</b>
L0F2B:  SET     6,(IY+$3B)      ; sv CDFLAG
        JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0207">L0207</a>           ; to SLOW/FAST

; ---------------------------
; THE <b><font color=3D"#333388">'PAUSE'</font></b> COMMAND ROUTINE
; ---------------------------

<a name=3D"L0F32"></a>;; <b>PAUSE</b>
L0F32:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0EA7">L0EA7</a>           ; routine FIND-INT
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L02E7">L02E7</a>           ; routine SET-FAST
        LD      H,B             ;
        LD      L,C             ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L022D">L022D</a>           ; routine DISPLAY-P

        LD      (IY+$35),$FF    ; sv FRAMES_hi

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0207">L0207</a>           ; routine SLOW/FAST
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0F4B">L0F4B</a>           ; routine DEBOUNCE

; ----------------------
; THE <b><font color=3D"#333388">'BREAK'</font></b> SUBROUTINE
; ----------------------
;
;

<a name=3D"L0F46"></a>;; <b>BREAK-1</b>
L0F46:  LD      A,$7F           ; read port $7FFE - keys B,N,M,.,SPACE.
        IN      A,($FE)         ;
        RRA                     ; carry will be set if space not pressed.

; -------------------------
; THE <b><font color=3D"#333388">'DEBOUNCE'</font></b> SUBROUTINE
; -------------------------
;
;

<a name=3D"L0F4B"></a>;; <b>DEBOUNCE</b>
L0F4B:  RES     0,(IY+$3B)      ; update system variable CDFLAG
        LD      A,$FF           ;
        LD      ($4027),A       ; update system variable DEBOUNCE
        RET                     ; return.


; -------------------------
; THE <b><font color=3D"#333388">'SCANNING'</font></b> SUBROUTINE
; -------------------------
; This recursive routine is where the ZX81 gets its power. Provided there i=
s
; enough memory it can evaluate an expression of unlimited complexity.
; <font color=3D"#9900FF">Note.</font> there is no unary plus so, as on the=
 ZX80, PRINT +1 gives a syntax error.
; PRINT +1 works on the Spectrum but so too does PRINT + "STRING".

<a name=3D"L0F55"></a>;; <b>SCANNING</b>
L0F55:  RST     18H             ; GET-CHAR
        LD      B,$00           ; set B register to zero.
        PUSH    BC              ; stack zero as a priority end-marker.

<a name=3D"L0F59"></a>;; <b>S-LOOP-1</b>
L0F59:  CP      $40             ; compare to the 'RND' character
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0F8C">L0F8C</a>        ; forward, if not, to S-TEST-PI

; ------------------
; THE <b><font color=3D"#333388">'RND'</font></b> FUNCTION
; ------------------

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0DA6">L0DA6</a>           ; routine SYNTAX-Z
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0F8A">L0F8A</a>         ; forward if checking syntax to S-JPI-END

        LD      BC,($4032)      ; sv SEED_lo
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L1520">L1520</a>           ; routine STACK-BC

        RST     28H             ;; FP-CALC
        DEFB    $A1             ;;stk-one
        DEFB    $0F             ;;addition
        DEFB    $30             ;;stk-data
        DEFB    $37             ;;Exponent: $87, Bytes: 1
        DEFB    $16             ;;(+00,+00,+00)
        DEFB    $04             ;;multiply
        DEFB    $30             ;;stk-data
        DEFB    $80             ;;Bytes: 3
        DEFB    $41             ;;Exponent $91
        DEFB    $00,$00,$80     ;;(+00)
        DEFB    $2E             ;;n-mod-m
        DEFB    $02             ;;delete
        DEFB    $A1             ;;stk-one
        DEFB    $03             ;;subtract
        DEFB    $2D             ;;duplicate
        DEFB    $34             ;;end-calc

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L158A">L158A</a>           ; routine FP-TO-BC
        LD      ($4032),BC      ; update the SEED system variable.
        LD      A,(HL)          ; HL addresses the exponent of the last val=
ue.
        AND     A               ; test for zero
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0F8A">L0F8A</a>         ; forward, if so, to S-JPI-END

        SUB     $10             ; else reduce exponent by sixteen
        LD      (HL),A          ; thus dividing by 65536 for last value.

<a name=3D"L0F8A"></a>;; <b>S-JPI-END</b>
L0F8A:  JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0F99">L0F99</a>           ; forward to S-PI-END

; ---

<a name=3D"L0F8C"></a>;; <b>S-TEST-PI</b>
L0F8C:  CP      $42             ; the 'PI' character
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0F9D">L0F9D</a>        ; forward, if not, to S-TST-INK

; -------------------
; THE <b><font color=3D"#333388">'PI'</font></b> EVALUATION
; -------------------

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0DA6">L0DA6</a>           ; routine SYNTAX-Z
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0F99">L0F99</a>         ; forward if checking syntax to S-PI-END


        RST     28H             ;; FP-CALC
        DEFB    $A3             ;;stk-pi/2
        DEFB    $34             ;;end-calc

        INC     (HL)            ; double the exponent giving PI on the stac=
k.

<a name=3D"L0F99"></a>;; <b>S-PI-END</b>
L0F99:  RST     20H             ; NEXT-CHAR advances character pointer.

        JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L1083">L1083</a>           ; jump forward to S-NUMERIC to set the flag
                                ; to signal numeric result before advancing=
.

; ---

<a name=3D"L0F9D"></a>;; <b>S-TST-INK</b>
L0F9D:  CP      $41             ; compare to character 'INKEY$'
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0FB2">L0FB2</a>        ; forward, if not, to S-ALPHANUM

; -----------------------
; THE <b><font color=3D"#333388">'INKEY$'</font></b> EVALUATION
; -----------------------

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L02BB">L02BB</a>           ; routine KEYBOARD
        LD      B,H             ;
        LD      C,L             ;
        LD      D,C             ;
        INC     D               ;
        CALL    NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L07BD">L07BD</a>        ; routine DECODE
        LD      A,D             ;
        ADC     A,D             ;
        LD      B,D             ;
        LD      C,A             ;
        EX      DE,HL           ;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0FED">L0FED</a>           ; forward to S-STRING

; ---

<a name=3D"L0FB2"></a>;; <b>S-ALPHANUM</b>
L0FB2:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L14D2">L14D2</a>           ; routine ALPHANUM
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1025">L1025</a>         ; forward, if alphanumeric to S-LTR-DGT

        CP      $1B             ; is character a '.' ?
        JP      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1047">L1047</a>         ; jump forward if so to S-DECIMAL

        LD      BC,$09D8        ; prepare priority 09, operation 'subtract'
        CP      $16             ; is character unary minus '-' ?
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1020">L1020</a>         ; forward, if so, to S-PUSH-PO

        CP      $10             ; is character a '(' ?
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0FD6">L0FD6</a>        ; forward if not to S-QUOTE

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0049">L0049</a>           ; routine CH-ADD+1 advances character pointe=
r.

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0F55">L0F55</a>           ; recursively call routine SCANNING to
                                ; evaluate the sub-expression.

        CP      $11             ; is subsequent character a ')' ?
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0FFF">L0FFF</a>        ; forward if not to S-RPT-C


        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0049">L0049</a>           ; routine CH-ADD+1  advances.
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0FF8">L0FF8</a>           ; relative jump to S-JP-CONT3 and then S-CON=
T3

; ---

; consider a quoted string e.g. PRINT "Hooray!"
; <font color=3D"#9900FF">Note.</font> quotes are not allowed within a stri=
ng.

<a name=3D"L0FD6"></a>;; <b>S-QUOTE</b>
L0FD6:  CP      $0B             ; is character a quote (") ?
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L1002">L1002</a>        ; forward, if not, to S-FUNCTION

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0049">L0049</a>           ; routine CH-ADD+1 advances
        PUSH    HL              ; * save start of string.
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0FE3">L0FE3</a>           ; forward to S-QUOTE-S

; ---


<a name=3D"L0FE0"></a>;; <b>S-Q-AGAIN</b>
L0FE0:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0049">L0049</a>           ; routine CH-ADD+1

<a name=3D"L0FE3"></a>;; <b>S-QUOTE-S</b>
L0FE3:  CP      $0B             ; is character a '"' ?
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0FFB">L0FFB</a>        ; forward if not to S-Q-NL

        POP     DE              ; * retrieve start of string
        AND     A               ; prepare to subtract.
        SBC     HL,DE           ; subtract start from current position.
        LD      B,H             ; transfer this length
        LD      C,L             ; to the BC register pair.

<a name=3D"L0FED"></a>;; <b>S-STRING</b>
L0FED:  LD      HL,$4001        ; address system variable FLAGS
        RES     6,(HL)          ; signal string result
        BIT     7,(HL)          ; test if checking syntax.

        CALL    NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L12C3">L12C3</a>        ; in run-time routine STK-STO-$ stacks the
                                ; string descriptor - start DE, length BC.

        RST     20H             ; NEXT-CHAR advances pointer.

<a name=3D"L0FF8"></a>;; <b>S-J-CONT-3</b>
L0FF8:  JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L1088">L1088</a>           ; jump to S-CONT-3

; ---

; A string with no terminating quote has to be considered.

<a name=3D"L0FFB"></a>;; <b>S-Q-NL</b>
L0FFB:  CP      $76             ; compare to NEWLINE
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0FE0">L0FE0</a>        ; loop back if not to S-Q-AGAIN

<a name=3D"L0FFF"></a>;; <b>S-RPT-C</b>
L0FFF:  JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0D9A">L0D9A</a>           ; to REPORT-C

; ---

<a name=3D"L1002"></a>;; <b>S-FUNCTION</b>
L1002:  SUB     $C4             ; subtract 'CODE' reducing codes
                                ; CODE thru '&lt;&gt;' to range $00 - $XX
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0FFF">L0FFF</a>         ; back, if less, to S-RPT-C

; test for NOT the last function in character set.

        LD      BC,$04EC        ; prepare priority $04, operation 'not'
        CP      $13             ; compare to 'NOT'  ( - CODE)
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1020">L1020</a>         ; forward, if so, to S-PUSH-PO

        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0FFF">L0FFF</a>        ; back with anything higher to S-RPT-C

; else is a function 'CODE' thru 'CHR$'

        LD      B,$10           ; priority sixteen binds all functions to
                                ; arguments removing the need for brackets.

        ADD     A,$D9           ; add $D9 to give range $D9 thru $EB
                                ; bit 6 is set to show numeric argument.
                                ; bit 7 is set to show numeric result.

; now adjust these default argument/result indicators.

        LD      C,A             ; save code in C

        CP      $DC             ; separate 'CODE', 'VAL', 'LEN'
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L101A">L101A</a>        ; skip forward if string operand to S-NO-TO-=
$

        RES     6,C             ; signal string operand.

<a name=3D"L101A"></a>;; <b>S-NO-TO-$</b>
L101A:  CP      $EA             ; isolate top of range 'STR$' and 'CHR$'
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1020">L1020</a>         ; skip forward with others to S-PUSH-PO

        RES     7,C             ; signal string result.

<a name=3D"L1020"></a>;; <b>S-PUSH-PO</b>
L1020:  PUSH    BC              ; push the priority/operation

        RST     20H             ; NEXT-CHAR
        JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0F59">L0F59</a>           ; jump back to S-LOOP-1

; ---

<a name=3D"L1025"></a>;; <b>S-LTR-DGT</b>
L1025:  CP      $26             ; compare to 'A'.
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1047">L1047</a>         ; forward if less to S-DECIMAL

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L111C">L111C</a>           ; routine LOOK-VARS
        JP      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0D4B">L0D4B</a>         ; back if not found to REPORT-2
                                ; a variable is always 'found' when checkin=
g
                                ; syntax.

        CALL    Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L11A7">L11A7</a>         ; routine STK-VAR stacks string parameters o=
r
                                ; returns cell location if numeric.

        LD      A,($4001)       ; fetch FLAGS
        CP      $C0             ; compare to numeric result/numeric operand
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1087">L1087</a>         ; forward if not numeric to S-CONT-2

        INC     HL              ; address numeric contents of variable.
        LD      DE,($401C)      ; set destination to STKEND
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L19F6">L19F6</a>           ; routine MOVE-FP stacks the five bytes
        EX      DE,HL           ; transfer new free location from DE to HL.
        LD      ($401C),HL      ; update STKEND system variable.
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L1087">L1087</a>           ; forward to S-CONT-2

; ---

; The Scanning Decimal routine is invoked when a decimal point or digit is
; found in the expression.
; When checking syntax, then the 'hidden floating point' form is placed
; after the number in the BASIC line.
; In run-time, the digits are skipped and the floating point number is pick=
ed
; up.

<a name=3D"L1047"></a>;; <b>S-DECIMAL</b>
L1047:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0DA6">L0DA6</a>           ; routine SYNTAX-Z
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L106F">L106F</a>        ; forward in run-time to S-STK-DEC

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L14D9">L14D9</a>           ; routine DEC-TO-FP

        RST     18H             ; GET-CHAR advances HL past digits
        LD      BC,$0006        ; six locations are required.
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L099E">L099E</a>           ; routine MAKE-ROOM
        INC     HL              ; point to first new location
        LD      (HL),$7E        ; insert the number marker 126 decimal.
        INC     HL              ; increment
        EX      DE,HL           ; transfer destination to DE.
        LD      HL,($401C)      ; set HL from STKEND which points to the
                                ; first location after the 'last value'
        LD      C,$05           ; five bytes to move.
        AND     A               ; clear carry.
        SBC     HL,BC           ; subtract five pointing to 'last value'.
        LD      ($401C),HL      ; update STKEND thereby 'deleting the value=
.

        LDIR                    ; copy the five value bytes.

        EX      DE,HL           ; basic pointer to HL which may be white-sp=
ace
                                ; following the number.
        DEC     HL              ; now points to last of five bytes.
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L004C">L004C</a>           ; routine TEMP-PTR1 advances the character
                                ; address skipping any white-space.
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L1083">L1083</a>           ; forward to S-NUMERIC
                                ; to signal a numeric result.

; ---

; In run-time the branch is here when a digit or point is encountered.

<a name=3D"L106F"></a>;; <b>S-STK-DEC</b>
L106F:  RST     20H             ; NEXT-CHAR
        CP      $7E             ; compare to 'number marker'
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L106F">L106F</a>        ; loop back until found to S-STK-DEC
                                ; skipping all the digits.

        INC     HL              ; point to first of five hidden bytes.
        LD      DE,($401C)      ; set destination from STKEND system variab=
le
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L19F6">L19F6</a>           ; routine MOVE-FP stacks the number.
        LD      ($401C),DE      ; update system variable STKEND.
        LD      ($4016),HL      ; update system variable CH_ADD.

<a name=3D"L1083"></a>;; <b>S-NUMERIC</b>
L1083:  SET     6,(IY+$01)      ; update FLAGS  - Signal numeric result

<a name=3D"L1087"></a>;; <b>S-CONT-2</b>
L1087:  RST     18H             ; GET-CHAR

<a name=3D"L1088"></a>;; <b>S-CONT-3</b>
L1088:  CP      $10             ; compare to opening bracket '('
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L1098">L1098</a>        ; forward if not to S-OPERTR

        BIT     6,(IY+$01)      ; test FLAGS  - Numeric or string result?
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L10BC">L10BC</a>        ; forward if numeric to S-LOOP

; else is a string

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L1263">L1263</a>           ; routine SLICING

        RST     20H             ; NEXT-CHAR
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L1088">L1088</a>           ; back to S-CONT-3

; ---

; the character is now manipulated to form an equivalent in the table of
; calculator literals. This is quite cumbersome and in the ZX Spectrum a
; simple look-up table was introduced at this point.

<a name=3D"L1098"></a>;; <b>S-OPERTR</b>
L1098:  LD      BC,$00C3        ; prepare operator 'subtract' as default.
                                ; also set B to zero for later indexing.

        CP      $12             ; is character '&gt;' ?
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L10BC">L10BC</a>         ; forward if less to S-LOOP as
                                ; we have reached end of meaningful express=
ion

        SUB     $16             ; is character '-' ?
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L10A7">L10A7</a>        ; forward with - * / and '**' '&lt;&gt;' to =
SUBMLTDIV

        ADD     A,$0D           ; increase others by thirteen
                                ; $09 '&gt;' thru $0C '+'
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L10B5">L10B5</a>           ; forward to GET-PRIO

; ---

<a name=3D"L10A7"></a>;; <b>SUBMLTDIV</b>
L10A7:  CP      $03             ; isolate $00 '-', $01 '*', $02 '/'
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L10B5">L10B5</a>         ; forward if so to GET-PRIO

; else possibly originally $D8 '**' thru $DD '&lt;&gt;' already reduced by =
$16

        SUB     $C2             ; giving range $00 to $05
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L10BC">L10BC</a>         ; forward if less to S-LOOP

        CP      $06             ; test the upper limit for nonsense also
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L10BC">L10BC</a>        ; forward if so to S-LOOP

        ADD     A,$03           ; increase by 3 to give combined operators =
of

                                ; $00 '-'
                                ; $01 '*'
                                ; $02 '/'

                                ; $03 '**'
                                ; $04 'OR'
                                ; $05 'AND'
                                ; $06 '&lt;=3D'
                                ; $07 '&gt;=3D'
                                ; $08 '&lt;&gt;'

                                ; $09 '&gt;'
                                ; $0A '&lt;'
                                ; $0B '=3D'
                                ; $0C '+'

<a name=3D"L10B5"></a>;; <b>GET-PRIO</b>
L10B5:  ADD     A,C             ; add to default operation 'sub' ($C3)
        LD      C,A             ; and place in operator byte - C.

        LD      HL,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L110F">L110F</a> - $C3  ; theoretical base of the priorities table.
        ADD     HL,BC           ; add C ( B is zero)
        LD      B,(HL)          ; pick up the priority in B

<a name=3D"L10BC"></a>;; <b>S-LOOP</b>
L10BC:  POP     DE              ; restore previous
        LD      A,D             ; load A with priority.
        CP      B               ; is present priority higher
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L10ED">L10ED</a>         ; forward if so to S-TIGHTER

        AND     A               ; are both priorities zero
        JP      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0018">L0018</a>         ; exit if zero via GET-CHAR

        PUSH    BC              ; stack present values
        PUSH    DE              ; stack last values
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0DA6">L0DA6</a>           ; routine SYNTAX-Z
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L10D5">L10D5</a>         ; forward is checking syntax to S-SYNTEST

        LD      A,E             ; fetch last operation
        AND     $3F             ; mask off the indicator bits to give true
                                ; calculator literal.
        LD      B,A             ; place in the B register for BREG

; perform the single operation

        RST     28H             ;; FP-CALC
        DEFB    $37             ;;fp-calc-2
        DEFB    $34             ;;end-calc

        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L10DE">L10DE</a>           ; forward to S-RUNTEST

; ---

<a name=3D"L10D5"></a>;; <b>S-SYNTEST</b>
L10D5:  LD      A,E             ; transfer masked operator to A
        XOR     (IY+$01)        ; XOR with FLAGS like results will reset bi=
t 6
        AND     $40             ; test bit 6

<a name=3D"L10DB"></a>;; <b>S-RPORT-C</b>
L10DB:  JP      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0D9A">L0D9A</a>        ; back to REPORT-C if results do not agree.

; ---

; in run-time impose bit 7 of the operator onto bit 6 of the FLAGS

<a name=3D"L10DE"></a>;; <b>S-RUNTEST</b>
L10DE:  POP     DE              ; restore last operation.
        LD      HL,$4001        ; address system variable FLAGS
        SET     6,(HL)          ; presume a numeric result
        BIT     7,E             ; test expected result in operation
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L10EA">L10EA</a>        ; forward if numeric to S-LOOPEND

        RES     6,(HL)          ; reset to signal string result

<a name=3D"L10EA"></a>;; <b>S-LOOPEND</b>
L10EA:  POP     BC              ; restore present values
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L10BC">L10BC</a>           ; back to S-LOOP

; ---

<a name=3D"L10ED"></a>;; <b>S-TIGHTER</b>
L10ED:  PUSH    DE              ; push last values and consider these

        LD      A,C             ; get the present operator.
        BIT     6,(IY+$01)      ; test FLAGS  - Numeric or string result?
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L110A">L110A</a>        ; forward if numeric to S-NEXT

        AND     $3F             ; strip indicator bits to give clear litera=
l.
        ADD     A,$08           ; add eight - augmenting numeric to equival=
ent
                                ; string literals.
        LD      C,A             ; place plain literal back in C.
        CP      $10             ; compare to 'AND'
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L1102">L1102</a>        ; forward if not to S-NOT-AND

        SET     6,C             ; set the numeric operand required for 'AND=
'
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L110A">L110A</a>           ; forward to S-NEXT

; ---

<a name=3D"L1102"></a>;; <b>S-NOT-AND</b>
L1102:  JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L10DB">L10DB</a>         ; back if less than 'AND' to S-RPORT-C
                                ; Nonsense if '-', '*' etc.

        CP      $17             ; compare to 'strs-add' literal
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L110A">L110A</a>         ; forward if so signaling string result

        SET     7,C             ; set bit to numeric (Boolean) for others.

<a name=3D"L110A"></a>;; <b>S-NEXT</b>
L110A:  PUSH    BC              ; stack 'present' values

        RST     20H             ; NEXT-CHAR
        JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0F59">L0F59</a>           ; jump back to S-LOOP-1



; -------------------------
; THE <b><font color=3D"#333388">'TABLE OF PRIORITIES'</font></b>
; -------------------------
;
;

<a name=3D"L110F"></a>;; <b>tbl-pri</b>
L110F:  DEFB    $06             ;       '-'
        DEFB    $08             ;       '*'
        DEFB    $08             ;       '/'
        DEFB    $0A             ;       '**'
        DEFB    $02             ;       'OR'
        DEFB    $03             ;       'AND'
        DEFB    $05             ;       '&lt;=3D'
        DEFB    $05             ;       '&gt;=3D'
        DEFB    $05             ;       '&lt;&gt;'
        DEFB    $05             ;       '&gt;'
        DEFB    $05             ;       '&lt;'
        DEFB    $05             ;       '=3D'
        DEFB    $06             ;       '+'


; --------------------------
; THE <b><font color=3D"#333388">'LOOK-VARS'</font></b> SUBROUTINE
; --------------------------
;
;

<a name=3D"L111C"></a>;; <b>LOOK-VARS</b>
L111C:  SET     6,(IY+$01)      ; sv FLAGS  - Signal numeric result

        RST     18H             ; GET-CHAR
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L14CE">L14CE</a>           ; routine ALPHA
        JP      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0D9A">L0D9A</a>        ; to REPORT-C

        PUSH    HL              ;
        LD      C,A             ;

        RST     20H             ; NEXT-CHAR
        PUSH    HL              ;
        RES     5,C             ;
        CP      $10             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1148">L1148</a>         ; to V-SYN/RUN

        SET     6,C             ;
        CP      $0D             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1143">L1143</a>         ; forward to V-STR-VAR

        SET     5,C             ;

<a name=3D"L1139"></a>;; <b>V-CHAR</b>
L1139:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L14D2">L14D2</a>           ; routine ALPHANUM
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L1148">L1148</a>        ; forward when not to V-RUN/SYN

        RES     6,C             ;

        RST     20H             ; NEXT-CHAR
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L1139">L1139</a>           ; loop back to V-CHAR

; ---

<a name=3D"L1143"></a>;; <b>V-STR-VAR</b>
L1143:  RST     20H             ; NEXT-CHAR
        RES     6,(IY+$01)      ; sv FLAGS  - Signal string result

<a name=3D"L1148"></a>;; <b>V-RUN/SYN</b>
L1148:  LD      B,C             ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0DA6">L0DA6</a>           ; routine SYNTAX-Z
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L1156">L1156</a>        ; forward to V-RUN

        LD      A,C             ;
        AND     $E0             ;
        SET     7,A             ;
        LD      C,A             ;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L118A">L118A</a>           ; forward to V-SYNTAX

; ---

<a name=3D"L1156"></a>;; <b>V-RUN</b>
L1156:  LD      HL,($4010)      ; sv VARS

<a name=3D"L1159"></a>;; <b>V-EACH</b>
L1159:  LD      A,(HL)          ;
        AND     $7F             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1188">L1188</a>         ; to V-80-BYTE

        CP      C               ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L1180">L1180</a>        ; to V-NEXT

        RLA                     ;
        ADD     A,A             ;
        JP      P,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1195">L1195</a>         ; to V-FOUND-2

        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1195">L1195</a>         ; to V-FOUND-2

        POP     DE              ;
        PUSH    DE              ;
        PUSH    HL              ;

<a name=3D"L116B"></a>;; <b>V-MATCHES</b>
L116B:  INC     HL              ;

<a name=3D"L116C"></a>;; <b>V-SPACES</b>
L116C:  LD      A,(DE)          ;
        INC     DE              ;
        AND     A               ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L116C">L116C</a>         ; back to V-SPACES

        CP      (HL)            ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L116B">L116B</a>         ; back to V-MATCHES

        OR      $80             ;
        CP      (HL)            ;
        JR       NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/z=
x81.htm#L117F">L117F</a>       ; forward to V-GET-PTR

        LD      A,(DE)          ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L14D2">L14D2</a>           ; routine ALPHANUM
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L1194">L1194</a>        ; forward to V-FOUND-1

<a name=3D"L117F"></a>;; <b>V-GET-PTR</b>
L117F:  POP     HL              ;

<a name=3D"L1180"></a>;; <b>V-NEXT</b>
L1180:  PUSH    BC              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L09F2">L09F2</a>           ; routine NEXT-ONE
        EX      DE,HL           ;
        POP     BC              ;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L1159">L1159</a>           ; back to V-EACH

; ---

<a name=3D"L1188"></a>;; <b>V-80-BYTE</b>
L1188:  SET     7,B             ;

<a name=3D"L118A"></a>;; <b>V-SYNTAX</b>
L118A:  POP     DE              ;

        RST     18H             ; GET-CHAR
        CP      $10             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1199">L1199</a>         ; forward to V-PASS

        SET     5,B             ;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L11A1">L11A1</a>           ; forward to V-END

; ---

<a name=3D"L1194"></a>;; <b>V-FOUND-1</b>
L1194:  POP     DE              ;

<a name=3D"L1195"></a>;; <b>V-FOUND-2</b>
L1195:  POP     DE              ;
        POP     DE              ;
        PUSH    HL              ;

        RST     18H             ; GET-CHAR

<a name=3D"L1199"></a>;; <b>V-PASS</b>
L1199:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L14D2">L14D2</a>           ; routine ALPHANUM
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L11A1">L11A1</a>        ; forward if not alphanumeric to V-END


        RST     20H             ; NEXT-CHAR
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L1199">L1199</a>           ; back to V-PASS

; ---

<a name=3D"L11A1"></a>;; <b>V-END</b>
L11A1:  POP     HL              ;
        RL      B               ;
        BIT     6,B             ;
        RET                     ;

; ------------------------
; THE <b><font color=3D"#333388">'STK-VAR'</font></b> SUBROUTINE
; ------------------------
;
;

<a name=3D"L11A7"></a>;; <b>STK-VAR</b>
L11A7:  XOR     A               ;
        LD      B,A             ;
        BIT     7,C             ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L11F8">L11F8</a>        ; forward to SV-COUNT

        BIT     7,(HL)          ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L11BF">L11BF</a>        ; forward to SV-ARRAYS

        INC     A               ;

<a name=3D"L11B2"></a>;; <b>SV-SIMPLE$</b>
L11B2:  INC     HL              ;
        LD      C,(HL)          ;
        INC     HL              ;
        LD      B,(HL)          ;
        INC     HL              ;
        EX      DE,HL           ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L12C3">L12C3</a>           ; routine STK-STO-$

        RST     18H             ; GET-CHAR
        JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L125A">L125A</a>           ; jump forward to SV-SLICE?

; ---

<a name=3D"L11BF"></a>;; <b>SV-ARRAYS</b>
L11BF:  INC     HL              ;
        INC     HL              ;
        INC     HL              ;
        LD      B,(HL)          ;
        BIT     6,C             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L11D1">L11D1</a>         ; forward to SV-PTR

        DEC     B               ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L11B2">L11B2</a>         ; forward to SV-SIMPLE$

        EX      DE,HL           ;

        RST     18H             ; GET-CHAR
        CP      $10             ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L1231">L1231</a>        ; forward to REPORT-3

        EX      DE,HL           ;

<a name=3D"L11D1"></a>;; <b>SV-PTR</b>
L11D1:  EX      DE,HL           ;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L11F8">L11F8</a>           ; forward to SV-COUNT

; ---

<a name=3D"L11D4"></a>;; <b>SV-COMMA</b>
L11D4:  PUSH    HL              ;

        RST     18H             ; GET-CHAR
        POP     HL              ;
        CP      $1A             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L11FB">L11FB</a>         ; forward to SV-LOOP

        BIT     7,C             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1231">L1231</a>         ; forward to REPORT-3

        BIT     6,C             ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L11E9">L11E9</a>        ; forward to SV-CLOSE

        CP      $11             ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L1223">L1223</a>        ; forward to SV-RPT-C


        RST     20H             ; NEXT-CHAR
        RET                     ;

; ---

<a name=3D"L11E9"></a>;; <b>SV-CLOSE</b>
L11E9:  CP      $11             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1259">L1259</a>         ; forward to SV-DIM

        CP      $DF             ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L1223">L1223</a>        ; forward to SV-RPT-C


<a name=3D"L11F1"></a>;; <b>SV-CH-ADD</b>
L11F1:  RST     18H             ; GET-CHAR
        DEC     HL              ;
        LD      ($4016),HL      ; sv CH_ADD
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L1256">L1256</a>           ; forward to SV-SLICE

; ---

<a name=3D"L11F8"></a>;; <b>SV-COUNT</b>
L11F8:  LD      HL,$0000        ;

<a name=3D"L11FB"></a>;; <b>SV-LOOP</b>
L11FB:  PUSH    HL              ;

        RST     20H             ; NEXT-CHAR
        POP     HL              ;
        LD      A,C             ;
        CP      $C0             ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L120C">L120C</a>        ; forward to SV-MULT


        RST     18H             ; GET-CHAR
        CP      $11             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1259">L1259</a>         ; forward to SV-DIM

        CP      $DF             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L11F1">L11F1</a>         ; back to SV-CH-ADD

<a name=3D"L120C"></a>;; <b>SV-MULT</b>
L120C:  PUSH    BC              ;
        PUSH    HL              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L12FF">L12FF</a>           ; routine DE,(DE+1)
        EX      (SP),HL         ;
        EX      DE,HL           ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L12DD">L12DD</a>           ; routine INT-EXP1
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1231">L1231</a>         ; forward to REPORT-3

        DEC     BC              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L1305">L1305</a>           ; routine GET-HL*DE
        ADD     HL,BC           ;
        POP     DE              ;
        POP     BC              ;
        DJNZ    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L11D4">L11D4</a>           ; loop back to SV-COMMA

        BIT     7,C             ;

<a name=3D"L1223"></a>;; <b>SV-RPT-C</b>
L1223:  JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L128B">L128B</a>        ; relative jump to SL-RPT-C

        PUSH    HL              ;
        BIT     6,C             ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L123D">L123D</a>        ; forward to SV-ELEM$

        LD      B,D             ;
        LD      C,E             ;

        RST     18H             ; GET-CHAR
        CP      $11             ; is character a ')' ?
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1233">L1233</a>         ; skip forward to SV-NUMBER


<a name=3D"L1231"></a>;; <b>REPORT-3</b>
L1231:  RST     08H             ; ERROR-1
        DEFB    $02             ; Error Report: Subscript wrong


<a name=3D"L1233"></a>;; <b>SV-NUMBER</b>
L1233:  RST     20H             ; NEXT-CHAR
        POP     HL              ;
        LD      DE,$0005        ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L1305">L1305</a>           ; routine GET-HL*DE
        ADD     HL,BC           ;
        RET                     ; return                            &gt;&gt=
;

; ---

<a name=3D"L123D"></a>;; <b>SV-ELEM$</b>
L123D:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L12FF">L12FF</a>           ; routine DE,(DE+1)
        EX      (SP),HL         ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L1305">L1305</a>           ; routine GET-HL*DE
        POP     BC              ;
        ADD     HL,BC           ;
        INC     HL              ;
        LD      B,D             ;
        LD      C,E             ;
        EX      DE,HL           ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L12C2">L12C2</a>           ; routine STK-ST-0

        RST     18H             ; GET-CHAR
        CP      $11             ; is it ')' ?
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1259">L1259</a>         ; forward if so to SV-DIM

        CP      $1A             ; is it ',' ?
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L1231">L1231</a>        ; back if not to REPORT-3

<a name=3D"L1256"></a>;; <b>SV-SLICE</b>
L1256:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L1263">L1263</a>           ; routine SLICING

<a name=3D"L1259"></a>;; <b>SV-DIM</b>
L1259:  RST     20H             ; NEXT-CHAR

<a name=3D"L125A"></a>;; <b>SV-SLICE?</b>
L125A:  CP      $10             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1256">L1256</a>         ; back to SV-SLICE

        RES     6,(IY+$01)      ; sv FLAGS  - Signal string result
        RET                     ; return.

; ------------------------
; THE <b><font color=3D"#333388">'SLICING'</font></b> SUBROUTINE
; ------------------------
;
;

<a name=3D"L1263"></a>;; <b>SLICING</b>
L1263:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0DA6">L0DA6</a>           ; routine SYNTAX-Z
        CALL    NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L13F8">L13F8</a>        ; routine STK-FETCH

        RST     20H             ; NEXT-CHAR
        CP      $11             ; is it ')' ?
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L12BE">L12BE</a>         ; forward if so to SL-STORE

        PUSH    DE              ;
        XOR     A               ;
        PUSH    AF              ;
        PUSH    BC              ;
        LD      DE,$0001        ;

        RST     18H             ; GET-CHAR
        POP     HL              ;
        CP      $DF             ; is it 'TO' ?
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1292">L1292</a>         ; forward if so to SL-SECOND

        POP     AF              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L12DE">L12DE</a>           ; routine INT-EXP2
        PUSH    AF              ;
        LD      D,B             ;
        LD      E,C             ;
        PUSH    HL              ;

        RST     18H             ; GET-CHAR
        POP     HL              ;
        CP      $DF             ; is it 'TO' ?
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1292">L1292</a>         ; forward if so to SL-SECOND

        CP      $11             ;

<a name=3D"L128B"></a>;; <b>SL-RPT-C</b>
L128B:  JP      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0D9A">L0D9A</a>        ; to REPORT-C

        LD      H,D             ;
        LD      L,E             ;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L12A5">L12A5</a>           ; forward to SL-DEFINE

; ---

<a name=3D"L1292"></a>;; <b>SL-SECOND</b>
L1292:  PUSH    HL              ;

        RST     20H             ; NEXT-CHAR
        POP     HL              ;
        CP      $11             ; is it ')' ?
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L12A5">L12A5</a>         ; forward if so to SL-DEFINE

        POP     AF              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L12DE">L12DE</a>           ; routine INT-EXP2
        PUSH    AF              ;

        RST     18H             ; GET-CHAR
        LD      H,B             ;
        LD      L,C             ;
        CP      $11             ; is it ')' ?
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L128B">L128B</a>        ; back if not to SL-RPT-C

<a name=3D"L12A5"></a>;; <b>SL-DEFINE</b>
L12A5:  POP     AF              ;
        EX      (SP),HL         ;
        ADD     HL,DE           ;
        DEC     HL              ;
        EX      (SP),HL         ;
        AND     A               ;
        SBC     HL,DE           ;
        LD      BC,$0000        ;
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L12B9">L12B9</a>         ; forward to SL-OVER

        INC     HL              ;
        AND     A               ;
        JP      M,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1231">L1231</a>         ; jump back to REPORT-3

        LD      B,H             ;
        LD      C,L             ;

<a name=3D"L12B9"></a>;; <b>SL-OVER</b>
L12B9:  POP     DE              ;
        RES     6,(IY+$01)      ; sv FLAGS  - Signal string result

<a name=3D"L12BE"></a>;; <b>SL-STORE</b>
L12BE:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0DA6">L0DA6</a>           ; routine SYNTAX-Z
        RET     Z               ; return if checking syntax.

; --------------------------
; THE <b><font color=3D"#333388">'STK-STORE'</font></b> SUBROUTINE
; --------------------------
;
;

<a name=3D"L12C2"></a>;; <b>STK-ST-0</b>
L12C2:  XOR     A               ;

<a name=3D"L12C3"></a>;; <b>STK-STO-$</b>
L12C3:  PUSH    BC              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L19EB">L19EB</a>           ; routine TEST-5-SP
        POP     BC              ;
        LD      HL,($401C)      ; sv STKEND
        LD      (HL),A          ;
        INC     HL              ;
        LD      (HL),E          ;
        INC     HL              ;
        LD      (HL),D          ;
        INC     HL              ;
        LD      (HL),C          ;
        INC     HL              ;
        LD      (HL),B          ;
        INC     HL              ;
        LD      ($401C),HL      ; sv STKEND
        RES     6,(IY+$01)      ; update FLAGS - signal string result
        RET                     ; return.

; -------------------------
; THE <b><font color=3D"#333388">'INT EXP'</font></b> SUBROUTINES
; -------------------------
;
;

<a name=3D"L12DD"></a>;; <b>INT-EXP1</b>
L12DD:  XOR     A               ;

<a name=3D"L12DE"></a>;; <b>INT-EXP2</b>
L12DE:  PUSH    DE              ;
        PUSH    HL              ;
        PUSH    AF              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0D92">L0D92</a>           ; routine CLASS-6
        POP     AF              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0DA6">L0DA6</a>           ; routine SYNTAX-Z
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L12FC">L12FC</a>         ; forward if checking syntax to I-RESTORE

        PUSH    AF              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0EA7">L0EA7</a>           ; routine FIND-INT
        POP     DE              ;
        LD      A,B             ;
        OR      C               ;
        SCF                     ; Set Carry Flag
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L12F9">L12F9</a>         ; forward to I-CARRY

        POP     HL              ;
        PUSH    HL              ;
        AND     A               ;
        SBC     HL,BC           ;

<a name=3D"L12F9"></a>;; <b>I-CARRY</b>
L12F9:  LD      A,D             ;
        SBC     A,$00           ;

<a name=3D"L12FC"></a>;; <b>I-RESTORE</b>
L12FC:  POP     HL              ;
        POP     DE              ;
        RET                     ;

; --------------------------
; THE <b><font color=3D"#333388">'DE,(DE+1)'</font></b> SUBROUTINE
; --------------------------
; INDEX and LOAD Z80 subroutine.=20
; This emulates the 6800 processor instruction LDX 1,X which loads a two-by=
te
; value from memory into the register indexing it. Often these are hardly w=
orth
; the bother of writing as subroutines and this one doesn't save any time o=
r=20
; memory. The timing and space overheads have to be offset against the ease=
 of
; writing and the greater program readability from using such toolkit routi=
nes.

<a name=3D"L12FF"></a>;; <b>DE,(DE+1)</b>
L12FF:  EX      DE,HL           ; move index address into HL.
        INC     HL              ; increment to address word.
        LD      E,(HL)          ; pick up word low-order byte.
        INC     HL              ; index high-order byte and=20
        LD      D,(HL)          ; pick it up.
        RET                     ; return with DE =3D word.

; --------------------------
; THE <b><font color=3D"#333388">'GET-HL*DE'</font></b> SUBROUTINE
; --------------------------
;

<a name=3D"L1305"></a>;; <b>GET-HL*DE</b>
L1305:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0DA6">L0DA6</a>           ; routine SYNTAX-Z
        RET     Z               ;

        PUSH    BC              ;
        LD      B,$10           ;
        LD      A,H             ;
        LD      C,L             ;
        LD      HL,$0000        ;

<a name=3D"L1311"></a>;; <b>HL-LOOP</b>
L1311:  ADD     HL,HL           ;
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L131A">L131A</a>         ; forward with carry to HL-END

        RL      C               ;
        RLA                     ;
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L131D">L131D</a>        ; forward with no carry to HL-AGAIN

        ADD     HL,DE           ;

<a name=3D"L131A"></a>;; <b>HL-END</b>
L131A:  JP      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0ED3">L0ED3</a>         ; to REPORT-4

<a name=3D"L131D"></a>;; <b>HL-AGAIN</b>
L131D:  DJNZ    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L1311">L1311</a>           ; loop back to HL-LOOP

        POP     BC              ;
        RET                     ; return.

; --------------------
; THE <b><font color=3D"#333388">'LET'</font></b> SUBROUTINE
; --------------------
;
;

<a name=3D"L1321"></a>;; <b>LET</b>
L1321:  LD      HL,($4012)      ; sv DEST-lo
        BIT     1,(IY+$2D)      ; sv FLAGX
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L136E">L136E</a>         ; forward to L-EXISTS

        LD      BC,$0005        ;

<a name=3D"L132D"></a>;; <b>L-EACH-CH</b>
L132D:  INC     BC              ;

; check

<a name=3D"L132E"></a>;; <b>L-NO-SP</b>
L132E:  INC     HL              ;
        LD      A,(HL)          ;
        AND     A               ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L132E">L132E</a>         ; back to L-NO-SP

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L14D2">L14D2</a>           ; routine ALPHANUM
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L132D">L132D</a>         ; back to L-EACH-CH

        CP      $0D             ; is it '$' ?
        JP      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L13C8">L13C8</a>         ; forward if so to L-NEW$


        RST     30H             ; BC-SPACES
        PUSH    DE              ;
        LD      HL,($4012)      ; sv DEST
        DEC     DE              ;
        LD      A,C             ;
        SUB     $06             ;
        LD      B,A             ;
        LD      A,$40           ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1359">L1359</a>         ; forward to L-SINGLE

<a name=3D"L134B"></a>;; <b>L-CHAR</b>
L134B:  INC     HL              ;
        LD      A,(HL)          ;
        AND     A               ; is it a space ?
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L134B">L134B</a>         ; back to L-CHAR

        INC     DE              ;
        LD      (DE),A          ;
        DJNZ    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L134B">L134B</a>           ; loop back to L-CHAR

        OR      $80             ;
        LD      (DE),A          ;
        LD      A,$80           ;

<a name=3D"L1359"></a>;; <b>L-SINGLE</b>
L1359:  LD      HL,($4012)      ; sv DEST-lo
        XOR     (HL)            ;
        POP     HL              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L13E7">L13E7</a>           ; routine L-FIRST

<a name=3D"L1361"></a>;; <b>L-NUMERIC</b>
L1361:  PUSH    HL              ;

        RST     28H             ;; FP-CALC
        DEFB    $02             ;;delete
        DEFB    $34             ;;end-calc

        POP     HL              ;
        LD      BC,$0005        ;
        AND     A               ;
        SBC     HL,BC           ;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L13AE">L13AE</a>           ; forward to L-ENTER

; ---

<a name=3D"L136E"></a>;; <b>L-EXISTS</b>
L136E:  BIT     6,(IY+$01)      ; sv FLAGS  - Numeric or string result?
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L137A">L137A</a>         ; forward to L-DELETE$

        LD      DE,$0006        ;
        ADD     HL,DE           ;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L1361">L1361</a>           ; back to L-NUMERIC

; ---

<a name=3D"L137A"></a>;; <b>L-DELETE$</b>
L137A:  LD      HL,($4012)      ; sv DEST-lo
        LD      BC,($402E)      ; sv STRLEN_lo
        BIT     0,(IY+$2D)      ; sv FLAGX
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L13B7">L13B7</a>        ; forward to L-ADD$

        LD      A,B             ;
        OR      C               ;
        RET     Z               ;

        PUSH    HL              ;

        RST     30H             ; BC-SPACES
        PUSH    DE              ;
        PUSH    BC              ;
        LD      D,H             ;
        LD      E,L             ;
        INC     HL              ;
        LD      (HL),$00        ;
        LDDR                    ; Copy Bytes
        PUSH    HL              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L13F8">L13F8</a>           ; routine STK-FETCH
        POP     HL              ;
        EX      (SP),HL         ;
        AND     A               ;
        SBC     HL,BC           ;
        ADD     HL,BC           ;
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L13A3">L13A3</a>        ; forward to L-LENGTH

        LD      B,H             ;
        LD      C,L             ;

<a name=3D"L13A3"></a>;; <b>L-LENGTH</b>
L13A3:  EX      (SP),HL         ;
        EX      DE,HL           ;
        LD      A,B             ;
        OR      C               ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L13AB">L13AB</a>         ; forward if zero to L-IN-W/S

        LDIR                    ; Copy Bytes

<a name=3D"L13AB"></a>;; <b>L-IN-W/S</b>
L13AB:  POP     BC              ;
        POP     DE              ;
        POP     HL              ;

; ------------------------
; THE <b><font color=3D"#333388">'L-ENTER'</font></b> SUBROUTINE
; ------------------------
;

<a name=3D"L13AE"></a>;; <b>L-ENTER</b>
L13AE:  EX      DE,HL           ;
        LD      A,B             ;
        OR      C               ;
        RET     Z               ;

        PUSH    DE              ;
        LDIR                    ; Copy Bytes
        POP     HL              ;
        RET                     ; return.

; ---

<a name=3D"L13B7"></a>;; <b>L-ADD$</b>
L13B7:  DEC     HL              ;
        DEC     HL              ;
        DEC     HL              ;
        LD      A,(HL)          ;
        PUSH    HL              ;
        PUSH    BC              ;

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L13CE">L13CE</a>           ; routine L-STRING

        POP     BC              ;
        POP     HL              ;
        INC     BC              ;
        INC     BC              ;
        INC     BC              ;
        JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0A60">L0A60</a>           ; jump back to exit via RECLAIM-2

; ---

<a name=3D"L13C8"></a>;; <b>L-NEW$</b>
L13C8:  LD      A,$60           ; prepare mask %01100000
        LD      HL,($4012)      ; sv DEST-lo
        XOR     (HL)            ;

; -------------------------
; THE <b><font color=3D"#333388">'L-STRING'</font></b> SUBROUTINE
; -------------------------
;

<a name=3D"L13CE"></a>;; <b>L-STRING</b>
L13CE:  PUSH    AF              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L13F8">L13F8</a>           ; routine STK-FETCH
        EX      DE,HL           ;
        ADD     HL,BC           ;
        PUSH    HL              ;
        INC     BC              ;
        INC     BC              ;
        INC     BC              ;

        RST     30H             ; BC-SPACES
        EX      DE,HL           ;
        POP     HL              ;
        DEC     BC              ;
        DEC     BC              ;
        PUSH    BC              ;
        LDDR                    ; Copy Bytes
        EX      DE,HL           ;
        POP     BC              ;
        DEC     BC              ;
        LD      (HL),B          ;
        DEC     HL              ;
        LD      (HL),C          ;
        POP     AF              ;

<a name=3D"L13E7"></a>;; <b>L-FIRST</b>
L13E7:  PUSH    AF              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L14C7">L14C7</a>           ; routine REC-V80
        POP     AF              ;
        DEC     HL              ;
        LD      (HL),A          ;
        LD      HL,($401A)      ; sv STKBOT_lo
        LD      ($4014),HL      ; sv E_LINE_lo
        DEC     HL              ;
        LD      (HL),$80        ;
        RET                     ;

; --------------------------
; THE <b><font color=3D"#333388">'STK-FETCH'</font></b> SUBROUTINE
; --------------------------
; This routine fetches a five-byte value from the calculator stack
; reducing the pointer to the end of the stack by five.
; For a floating-point number the exponent is in A and the mantissa
; is the thirty-two bits EDCB.
; For strings, the start of the string is in DE and the length in BC.
; A is unused.

<a name=3D"L13F8"></a>;; <b>STK-FETCH</b>
L13F8:  LD      HL,($401C)      ; load HL from system variable STKEND

        DEC     HL              ;
        LD      B,(HL)          ;
        DEC     HL              ;
        LD      C,(HL)          ;
        DEC     HL              ;
        LD      D,(HL)          ;
        DEC     HL              ;
        LD      E,(HL)          ;
        DEC     HL              ;
        LD      A,(HL)          ;

        LD      ($401C),HL      ; set system variable STKEND to lower value=
.
        RET                     ; return.

; -------------------------
; THE <b><font color=3D"#333388">'DIM'</font></b> COMMAND ROUTINE
; -------------------------
; An array is created and initialized to zeros which is also the space
; character on the ZX81.

<a name=3D"L1409"></a>;; <b>DIM</b>
L1409:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L111C">L111C</a>           ; routine LOOK-VARS

<a name=3D"L140C"></a>;; <b>D-RPORT-C</b>
L140C:  JP      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0D9A">L0D9A</a>        ; to REPORT-C

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0DA6">L0DA6</a>           ; routine SYNTAX-Z
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L141C">L141C</a>        ; forward to D-RUN

        RES     6,C             ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L11A7">L11A7</a>           ; routine STK-VAR
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0D1D">L0D1D</a>           ; routine CHECK-END

<a name=3D"L141C"></a>;; <b>D-RUN</b>
L141C:  JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1426">L1426</a>         ; forward to D-LETTER

        PUSH    BC              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L09F2">L09F2</a>           ; routine NEXT-ONE
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0A60">L0A60</a>           ; routine RECLAIM-2
        POP     BC              ;

<a name=3D"L1426"></a>;; <b>D-LETTER</b>
L1426:  SET     7,C             ;
        LD      B,$00           ;
        PUSH    BC              ;
        LD      HL,$0001        ;
        BIT     6,C             ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L1434">L1434</a>        ; forward to D-SIZE

        LD      L,$05           ;

<a name=3D"L1434"></a>;; <b>D-SIZE</b>
L1434:  EX      DE,HL           ;

<a name=3D"L1435"></a>;; <b>D-NO-LOOP</b>
L1435:  RST     20H             ; NEXT-CHAR
        LD      H,$40           ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L12DD">L12DD</a>           ; routine INT-EXP1
        JP      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1231">L1231</a>         ; jump back to REPORT-3

        POP     HL              ;
        PUSH    BC              ;
        INC     H               ;
        PUSH    HL              ;
        LD      H,B             ;
        LD      L,C             ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L1305">L1305</a>           ; routine GET-HL*DE
        EX      DE,HL           ;

        RST     18H             ; GET-CHAR
        CP      $1A             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L1435">L1435</a>         ; back to D-NO-LOOP

        CP      $11             ; is it ')' ?
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L140C">L140C</a>        ; back if not to D-RPORT-C


        RST     20H             ; NEXT-CHAR
        POP     BC              ;
        LD      A,C             ;
        LD      L,B             ;
        LD      H,$00           ;
        INC     HL              ;
        INC     HL              ;
        ADD     HL,HL           ;
        ADD     HL,DE           ;
        JP      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0ED3">L0ED3</a>         ; jump to REPORT-4

        PUSH    DE              ;
        PUSH    BC              ;
        PUSH    HL              ;
        LD      B,H             ;
        LD      C,L             ;
        LD      HL,($4014)      ; sv E_LINE_lo
        DEC     HL              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L099E">L099E</a>           ; routine MAKE-ROOM
        INC     HL              ;
        LD       (HL),A         ;
        POP     BC              ;
        DEC     BC              ;
        DEC     BC              ;
        DEC     BC              ;
        INC     HL              ;
        LD      (HL),C          ;
        INC     HL              ;
        LD      (HL),B          ;
        POP     AF              ;
        INC     HL              ;
        LD      (HL),A          ;
        LD      H,D             ;
        LD      L,E             ;
        DEC     DE              ;
        LD      (HL),$00        ;
        POP     BC              ;
        LDDR                    ; Copy Bytes

<a name=3D"L147F"></a>;; <b>DIM-SIZES</b>
L147F:  POP     BC              ;
        LD      (HL),B          ;
        DEC     HL              ;
        LD      (HL),C          ;
        DEC     HL              ;
        DEC     A               ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L147F">L147F</a>        ; back to DIM-SIZES

        RET                     ; return.

; ---------------------
; THE <b><font color=3D"#333388">'RESERVE'</font></b> ROUTINE
; ---------------------
;
;

<a name=3D"L1488"></a>;; <b>RESERVE</b>
L1488:  LD      HL,($401A)      ; address STKBOT
        DEC     HL              ; now last byte of workspace
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L099E">L099E</a>           ; routine MAKE-ROOM
        INC     HL              ;
        INC     HL              ;
        POP     BC              ;
        LD      ($4014),BC      ; sv E_LINE_lo
        POP     BC              ;
        EX      DE,HL           ;
        INC     HL              ;
        RET                     ;

