; ------------------------------
; THE <b><font color=3D"#333388">'LOAD-SAVE UPDATE'</font></b> ROUTINE
; ------------------------------
;
;

<a name=3D"L01FC"></a>;; <b>LOAD/SAVE</b>
L01FC:  INC     HL              ;
        EX      DE,HL           ;
        LD      HL,($4014)      ; system variable edit line E_LINE.
        SCF                     ; set carry flag
        SBC     HL,DE           ;
        EX      DE,HL           ;
        RET     NC              ; return if more bytes to load/save.

        POP     HL              ; else drop return address

; ----------------------
; THE <b><font color=3D"#333388">'DISPLAY'</font></b> ROUTINES
; ----------------------
;
;

<a name=3D"L0207"></a>;; <b>SLOW/FAST</b>
L0207:  LD      HL,$403B        ; Address the system variable CDFLAG.
        LD      A,(HL)          ; Load value to the accumulator.
        RLA                     ; rotate bit 6 to position 7.
        XOR     (HL)            ; exclusive or with original bit 7.
        RLA                     ; rotate result out to carry.
        RET     NC              ; return if both bits were the same.

;   Now test if this really is a ZX81 or a ZX80 running the upgraded ROM.
;   The standard ZX80 did not have an NMI generator.

        LD      A,$7F           ; Load accumulator with %011111111
        EX      AF,AF'          ; save in AF'

        LD      B,$11           ; A counter within which an NMI should occu=
r
                                ; if this is a ZX81.
        OUT     ($FE),A         ; start the NMI generator.

;  Note that if this is a ZX81 then the NMI will increment AF'.

<a name=3D"L0216"></a>;; <b>LOOP-11</b>
L0216:  DJNZ    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0216">L0216</a>           ; self loop to give the NMI a chance to kick=
 in.
                                ; =3D 16*13 clock cycles + 8 =3D 216 clock =
cycles.

        OUT     ($FD),A         ; Turn off the NMI generator.
        EX      AF,AF'          ; bring back the AF' value.
        RLA                     ; test bit 7.
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0226">L0226</a>        ; forward, if bit 7 is still reset, to NO-SL=
OW.

;   If the AF' was incremented then the NMI generator works and SLOW mode c=
an
;   be set.

        SET     7,(HL)          ; Indicate SLOW mode - Compute and Display.

        PUSH    AF              ; *             Save Main Registers
        PUSH    BC              ; **
        PUSH    DE              ; ***
        PUSH    HL              ; ****

        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0229">L0229</a>           ; skip forward - to DISPLAY-1.

; ---

<a name=3D"L0226"></a>;; <b>NO-SLOW</b>
L0226:  RES     6,(HL)          ; reset bit 6 of CDFLAG.
        RET                     ; return.

; -----------------------
; THE <b><font color=3D"#333388">'MAIN DISPLAY'</font></b> LOOP
; -----------------------
; This routine is executed once for every frame displayed.

<a name=3D"L0229"></a>;; <b>DISPLAY-1</b>
L0229:  LD      HL,($4034)      ; fetch two-byte system variable FRAMES.
        DEC     HL              ; decrement frames counter.

<a name=3D"L022D"></a>;; <b>DISPLAY-P</b>
L022D:  LD      A,$7F           ; prepare a mask
        AND     H               ; pick up bits 6-0 of H.
        OR      L               ; and any bits of L.
        LD      A,H             ; reload A with all bits of H for PAUSE tes=
t.

;   Note both branches must take the same time.

        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0237">L0237</a>        ; (12/7) forward if bits 14-0 are not zero=
=20
                                ; to ANOTHER

        RLA                     ; (4) test bit 15 of FRAMES.
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0239">L0239</a>           ; (12) forward with result to OVER-NC

; ---

<a name=3D"L0237"></a>;; <b>ANOTHER</b>
L0237:  LD      B,(HL)          ; (7) <font color=3D"#9900FF">Note.</font> =
Harmless Nonsensical Timing weight.
        SCF                     ; (4) Set Carry Flag.

; <font color=3D"#9900FF">Note.</font> the branch to here takes either (12)=
(7)(4) cyles or (7)(4)(12) cycles.

<a name=3D"L0239"></a>;; <b>OVER-NC</b>
L0239:  LD      H,A             ; (4)  set H to zero
        LD      ($4034),HL      ; (16) update system variable FRAMES=20
        RET     NC              ; (11/5) return if FRAMES is in use by PAUS=
E=20
                                ; command.

<a name=3D"L023E"></a>;; <b>DISPLAY-2</b>
L023E:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L02BB">L02BB</a>           ; routine KEYBOARD gets the key row in H and=
=20
                                ; the column in L. Reading the ports also s=
tarts
                                ; the TV frame synchronization pulse. (VSYN=
C)

        LD      BC,($4025)      ; fetch the last key values read from LAST_=
K
        LD      ($4025),HL      ; update LAST_K with new values.

        LD      A,B             ; load A with previous column - will be $FF=
 if
                                ; there was no key.
        ADD     A,$02           ; adding two will set carry if no previous =
key.

        SBC     HL,BC           ; subtract with the carry the two key value=
s.

; If the same key value has been returned twice then HL will be zero.

        LD      A,($4027)       ; fetch system variable DEBOUNCE
        OR      H               ; and OR with both bytes of the difference
        OR      L               ; setting the zero flag for the upcoming br=
anch.

        LD      E,B             ; transfer the column value to E
        LD      B,$0B           ; and load B with eleven=20

        LD      HL,$403B        ; address system variable CDFLAG
        RES     0,(HL)          ; reset the rightmost bit of CDFLAG
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0264">L0264</a>        ; skip forward if debounce/diff &gt;0 to NO-=
KEY

        BIT     7,(HL)          ; test compute and display bit of CDFLAG
        SET     0,(HL)          ; set the rightmost bit of CDFLAG.
        RET     Z               ; return if bit 7 indicated fast mode.

        DEC     B               ; (4) decrement the counter.
        NOP                     ; (4) Timing - 4 clock cycles. ??
        SCF                     ; (4) Set Carry Flag

<a name=3D"L0264"></a>;; <b>NO-KEY</b>
L0264:  LD      HL,$4027        ; sv DEBOUNCE
        CCF                     ; Complement Carry Flag
        RL      B               ; rotate left B picking up carry
                                ;  C&lt;-76543210&lt;-C

<a name=3D"L026A"></a>;; <b>LOOP-B</b>
L026A:  DJNZ    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L026A">L026A</a>           ; self-loop while B&gt;0 to LOOP-B

        LD      B,(HL)          ; fetch value of DEBOUNCE to B
        LD      A,E             ; transfer column value
        CP      $FE             ;
        SBC     A,A             ;
        LD      B,$1F           ;
        OR      (HL)            ;
        AND     B               ;
        RRA                     ;
        LD      (HL),A          ;

        OUT     ($FF),A         ; end the TV frame synchronization pulse.

        LD      HL,($400C)      ; (12) set HL to the Display File from D_FI=
LE
        SET     7,H             ; (8) set bit 15 to address the echo displa=
y.

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0292">L0292</a>           ; (17) routine DISPLAY-3 displays the top se=
t=20
                                ; of blank lines.

; ---------------------
; THE <b><font color=3D"#333388">'VIDEO-1'</font></b> ROUTINE
; ---------------------

<a name=3D"L0281"></a>;; <b>R-IX-1</b>
L0281:  LD      A,R             ; (9)  Harmless Nonsensical Timing or somet=
hing
                                ;      very clever?
        LD      BC,$1901        ; (10) 25 lines, 1 scanline in first.
        LD      A,$F5           ; (7)  This value will be loaded into R and=
=20
                                ; ensures that the cycle starts at the righ=
t=20
                                ; part of the display  - after 32nd charact=
er=20
                                ; position.

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L02B5">L02B5</a>           ; (17) routine DISPLAY-5 completes the curre=
nt=20
                                ; blank line and then generates the display=
 of=20
                                ; the live picture using INT interrupts
                                ; The final interrupt returns to the next=
=20
                                ; address.

L028B:  DEC     HL              ; point HL to the last NEWLINE/HALT.

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0292">L0292</a>           ; routine DISPLAY-3 displays the bottom set =
of
                                ; blank lines.

; ---

<a name=3D"L028F"></a>;; <b>R-IX-2</b>
L028F:  JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0229">L0229</a>           ; JUMP back to DISPLAY-1

; ---------------------------------
; THE <b><font color=3D"#333388">'DISPLAY BLANK LINES'</font></b> ROUTINE=
=20
; ---------------------------------
;   This subroutine is called twice (see above) to generate first the blank=
=20
;   lines at the top of the television display and then the blank lines at =
the
;   bottom of the display.=20

<a name=3D"L0292"></a>;; <b>DISPLAY-3</b>
L0292:  POP     IX              ; pop the return address to IX register.
                                ; will be either L0281 or L028F - see above=
.

        LD      C,(IY+$28)      ; load C with value of system constant MARG=
IN.
        BIT     7,(IY+$3B)      ; test CDFLAG for compute and display.
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L02A9">L02A9</a>         ; forward, with FAST mode, to DISPLAY-4

        LD      A,C             ; move MARGIN to A  - 31d or 55d.
        NEG                     ; Negate
        INC     A               ;
        EX      AF,AF'          ; place negative count of blank lines in A'

        OUT     ($FE),A         ; enable the NMI generator.

        POP     HL              ; ****
        POP     DE              ; ***
        POP     BC              ; **
        POP     AF              ; *             Restore Main Registers

        RET                     ; return - end of interrupt.  Return is to=
=20
                                ; user's program - BASIC or machine code.
                                ; which will be interrupted by every NMI.

; ------------------------
; THE <b><font color=3D"#333388">'FAST MODE'</font></b> ROUTINES
; ------------------------

<a name=3D"L02A9"></a>;; <b>DISPLAY-4</b>
L02A9:  LD      A,$FC           ; (7)  load A with first R delay value
        LD      B,$01           ; (7)  one row only.

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L02B5">L02B5</a>           ; (17) routine DISPLAY-5

        DEC     HL              ; (6)  point back to the HALT.
        EX      (SP),HL         ; (19) Harmless Nonsensical Timing if paire=
d.
        EX      (SP),HL         ; (19) Harmless Nonsensical Timing.
        JP      (IX)            ; (8)  to L0281 or L028F

; --------------------------
; THE <b><font color=3D"#333388">'DISPLAY-5'</font></b> SUBROUTINE
; --------------------------
;   This subroutine is called from SLOW mode and FAST mode to generate the=
=20
;   central TV picture. With SLOW mode the R register is incremented, with
;   each instruction, to $F7 by the time it completes.  With fast mode, the=
=20
;   final R value will be $FF and an interrupt will occur as soon as the=20
;   Program Counter reaches the HALT.  (24 clock cycles)

<a name=3D"L02B5"></a>;; <b>DISPLAY-5</b>
L02B5:  LD      R,A             ; (9) Load R from A.    R =3D slow: $F5 fas=
t: $FC
        LD      A,$DD           ; (7) load future R value.        $F6      =
 $FD

        EI                      ; (4) Enable Interrupts           $F7      =
 $FE

        JP      (HL)            ; (4) jump to the echo display.   $F8      =
 $FF

; ----------------------------------
; THE <b><font color=3D"#333388">'KEYBOARD SCANNING'</font></b> SUBROUTINE
; ----------------------------------
; The keyboard is read during the vertical sync interval while no video is=
=20
; being displayed.  Reading a port with address bit 0 low i.e. $FE starts t=
he=20
; vertical sync pulse.

<a name=3D"L02BB"></a>;; <b>KEYBOARD</b>
L02BB:  LD      HL,$FFFF        ; (16) prepare a buffer to take key.
        LD      BC,$FEFE        ; (20) set BC to port $FEFE. The B register=
,=20
                                ;      with its single reset bit also acts =
as=20
                                ;      an 8-counter.
        IN      A,(C)           ; (11) read the port - all 16 bits are put =
on=20
                                ;      the address bus.  Start VSYNC pulse.
        OR      $01             ; (7)  set the rightmost bit so as to ignor=
e=20
                                ;      the SHIFT key.

<a name=3D"L02C5"></a>;; <b>EACH-LINE</b>
L02C5:  OR      $E0             ; [7] OR %11100000
        LD      D,A             ; [4] transfer to D.
        CPL                     ; [4] complement - only bits 4-0 meaningful=
 now.
        CP      $01             ; [7] sets carry if A is zero.
        SBC     A,A             ; [4] $FF if $00 else zero.
        OR      B               ; [7] $FF or port FE,FD,FB....
        AND     L               ; [4] unless more than one key, L will stil=
l be=20
                                ;     $FF. if more than one key is pressed =
then A is=20
                                ;     now invalid.
        LD      L,A             ; [4] transfer to L.

; now consider the column identifier.

        LD      A,H             ; [4] will be $FF if no previous keys.
        AND     D               ; [4] 111xxxxx
        LD      H,A             ; [4] transfer A to H

; since only one key may be pressed, H will, if valid, be one of
; 11111110, 11111101, 11111011, 11110111, 11101111
; reading from the outer column, say Q, to the inner column, say T.

        RLC     B               ; [8]  rotate the 8-counter/port address.
                                ;      sets carry if more to do.
        IN      A,(C)           ; [10] read another half-row.
                                ;      all five bits this time.

        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L02C5">L02C5</a>         ; [12](7) loop back, until done, to EACH-LIN=
E

;   The last row read is SHIFT,Z,X,C,V  for the second time.

        RRA                     ; (4) test the shift key - carry will be re=
set
                                ;     if the key is pressed.
        RL      H               ; (8) rotate left H picking up the carry gi=
ving
                                ;     column values -
                                ;        $FD, $FB, $F7, $EF, $DF.
                                ;     or $FC, $FA, $F6, $EE, $DE if shifted=
.

;   We now have H identifying the column and L identifying the row in the
;   keyboard matrix.

;   This is a good time to test if this is an American or British machine.
;   The US machine has an extra diode that causes bit 6 of a byte read from
;   a port to be reset.

        RLA                     ; (4) compensate for the shift test.
        RLA                     ; (4) rotate bit 7 out.
        RLA                     ; (4) test bit 6.

        SBC     A,A             ; (4)           $FF or $00 {USA}
        AND     $18             ; (7)           $18 or $00
        ADD     A,$1F           ; (7)           $37 or $1F

;   result is either 31 (USA) or 55 (UK) blank lines above and below the TV=
=20
;   picture.

        LD      ($4028),A       ; (13) update system variable MARGIN

        RET                     ; (10) return

; ------------------------------
; THE <b><font color=3D"#333388">'SET FAST MODE'</font></b> SUBROUTINE
; ------------------------------
;
;

<a name=3D"L02E7"></a>;; <b>SET-FAST</b>
L02E7:  BIT     7,(IY+$3B)      ; sv CDFLAG
        RET     Z               ;

        HALT                    ; Wait for Interrupt
        OUT     ($FD),A         ;
        RES     7,(IY+$3B)      ; sv CDFLAG
        RET                     ; return.


; --------------
; THE <b><font color=3D"#333388">'REPORT-F'</font></b>
; --------------

<a name=3D"L02F4"></a>;; <b>REPORT-F</b>
L02F4:  RST     08H             ; ERROR-1
        DEFB    $0E             ; Error Report: No Program Name supplied.

; --------------------------
; THE <b><font color=3D"#333388">'SAVE COMMAND'</font></b> ROUTINE
; --------------------------
;
;

<a name=3D"L02F6"></a>;; <b>SAVE</b>
L02F6:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L03A8">L03A8</a>           ; routine NAME
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L02F4">L02F4</a>         ; back with null name to REPORT-F above.

        EX      DE,HL           ;
        LD      DE,$12CB        ; five seconds timing value

<a name=3D"L02FF"></a>;; <b>HEADER</b>
L02FF:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0F46">L0F46</a>           ; routine BREAK-1
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0332">L0332</a>        ; to BREAK-2

<a name=3D"L0304"></a>;; <b>DELAY-1</b>
L0304:  DJNZ    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0304">L0304</a>           ; to DELAY-1

        DEC     DE              ;
        LD      A,D             ;
        OR      E               ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L02FF">L02FF</a>        ; back for delay to HEADER

<a name=3D"L030B"></a>;; <b>OUT-NAME</b>
L030B:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L031E">L031E</a>           ; routine OUT-BYTE
        BIT     7,(HL)          ; test for inverted bit.
        INC     HL              ; address next character of name.
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L030B">L030B</a>         ; back if not inverted to OUT-NAME

; now start saving the system variables onwards.

        LD      HL,$4009        ; set start of area to VERSN thereby
                                ; preserving RAMTOP etc.

<a name=3D"L0316"></a>;; <b>OUT-PROG</b>
L0316:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L031E">L031E</a>           ; routine OUT-BYTE

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L01FC">L01FC</a>           ; routine LOAD/SAVE                     &gt;=
&gt;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0316">L0316</a>           ; loop back to OUT-PROG

; -------------------------
; THE <b><font color=3D"#333388">'OUT-BYTE'</font></b> SUBROUTINE
; -------------------------
; This subroutine outputs a byte a bit at a time to a domestic tape recorde=
r.

<a name=3D"L031E"></a>;; <b>OUT-BYTE</b>
L031E:  LD      E,(HL)          ; fetch byte to be saved.
        SCF                     ; set carry flag - as a marker.

<a name=3D"L0320"></a>;; <b>EACH-BIT</b>
L0320:  RL      E               ;  C &lt; 76543210 &lt; C
        RET     Z               ; return when the marker bit has passed=20
                                ; right through.                        &gt=
;&gt;

        SBC     A,A             ; $FF if set bit or $00 with no carry.
        AND     $05             ; $05               $00
        ADD     A,$04           ; $09               $04
        LD      C,A             ; transfer timer to C. a set bit has a long=
er
                                ; pulse than a reset bit.

<a name=3D"L0329"></a>;; <b>PULSES</b>
L0329:  OUT     ($FF),A         ; pulse to cassette.
        LD      B,$23           ; set timing constant

<a name=3D"L032D"></a>;; <b>DELAY-2</b>
L032D:  DJNZ    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L032D">L032D</a>           ; self-loop to DELAY-2

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0F46">L0F46</a>           ; routine BREAK-1 test for BREAK key.

<a name=3D"L0332"></a>;; <b>BREAK-2</b>
L0332:  JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L03A6">L03A6</a>        ; forward with break to REPORT-D

        LD      B,$1E           ; set timing value.

<a name=3D"L0336"></a>;; <b>DELAY-3</b>
L0336:  DJNZ    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0336">L0336</a>           ; self-loop to DELAY-3

        DEC     C               ; decrement counter
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0329">L0329</a>        ; loop back to PULSES

<a name=3D"L033B"></a>;; <b>DELAY-4</b>
L033B:  AND     A               ; clear carry for next bit test.
        DJNZ    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L033B">L033B</a>           ; self loop to DELAY-4 (B is zero - 256)

        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0320">L0320</a>           ; loop back to EACH-BIT

; --------------------------
; THE <b><font color=3D"#333388">'LOAD COMMAND'</font></b> ROUTINE
; --------------------------
;
;

<a name=3D"L0340"></a>;; <b>LOAD</b>
L0340:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L03A8">L03A8</a>           ; routine NAME

; DE points to start of name in RAM.

        RL      D               ; pick up carry=20
        RRC     D               ; carry now in bit 7.

<a name=3D"L0347"></a>;; <b>NEXT-PROG</b>
L0347:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L034C">L034C</a>           ; routine IN-BYTE
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0347">L0347</a>           ; loop to NEXT-PROG

; ------------------------
; THE <b><font color=3D"#333388">'IN-BYTE'</font></b> SUBROUTINE
; ------------------------

<a name=3D"L034C"></a>;; <b>IN-BYTE</b>
L034C:  LD      C,$01           ; prepare an eight counter 00000001.

<a name=3D"L034E"></a>;; <b>NEXT-BIT</b>
L034E:  LD      B,$00           ; set counter to 256

<a name=3D"L0350"></a>;; <b>BREAK-3</b>
L0350:  LD      A,$7F           ; read the keyboard row=20
        IN      A,($FE)         ; with the SPACE key.

        OUT     ($FF),A         ; output signal to screen.

        RRA                     ; test for SPACE pressed.
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L03A2">L03A2</a>        ; forward if so to BREAK-4

        RLA                     ; reverse above rotation
        RLA                     ; test tape bit.
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0385">L0385</a>         ; forward if set to GET-BIT

        DJNZ    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0350">L0350</a>           ; loop back to BREAK-3

        POP     AF              ; drop the return address.
        CP      D               ; ugh.

<a name=3D"L0361"></a>;; <b>RESTART</b>
L0361:  JP      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L03E5">L03E5</a>        ; jump forward to INITIAL if D is zero=20
                                ; to reset the system
                                ; if the tape signal has timed out for exam=
ple
                                ; if the tape is stopped. Not just a simple=
=20
                                ; report as some system variables will have
                                ; been overwritten.

        LD      H,D             ; else transfer the start of name
        LD      L,E             ; to the HL register

<a name=3D"L0366"></a>;; <b>IN-NAME</b>
L0366:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L034C">L034C</a>           ; routine IN-BYTE is sort of recursion for n=
ame
                                ; part. received byte in C.
        BIT     7,D             ; is name the null string ?
        LD      A,C             ; transfer byte to A.
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0371">L0371</a>        ; forward with null string to MATCHING

        CP      (HL)            ; else compare with string in memory.
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0347">L0347</a>        ; back with mis-match to NEXT-PROG
                                ; (seemingly out of subroutine but return=
=20
                                ; address has been dropped).


<a name=3D"L0371"></a>;; <b>MATCHING</b>
L0371:  INC     HL              ; address next character of name
        RLA                     ; test for inverted bit.
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0366">L0366</a>        ; back if not to IN-NAME

; the name has been matched in full.=20
; proceed to load the data but first increment the high byte of E_LINE, whi=
ch
; is one of the system variables to be loaded in. Since the low byte is loa=
ded
; before the high byte, it is possible that, at the in-between stage, a fal=
se
; value could cause the load to end prematurely - see  LOAD/SAVE check.

        INC     (IY+$15)        ; increment system variable E_LINE_hi.
        LD      HL,$4009        ; start loading at system variable VERSN.

<a name=3D"L037B"></a>;; <b>IN-PROG</b>
L037B:  LD      D,B             ; set D to zero as indicator.
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L034C">L034C</a>           ; routine IN-BYTE loads a byte
        LD      (HL),C          ; insert assembled byte in memory.
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L01FC">L01FC</a>           ; routine LOAD/SAVE                     &gt;=
&gt;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L037B">L037B</a>           ; loop back to IN-PROG

; ---

; this branch assembles a full byte before exiting normally
; from the IN-BYTE subroutine.

<a name=3D"L0385"></a>;; <b>GET-BIT</b>
L0385:  PUSH    DE              ; save the=20
        LD      E,$94           ; timing value.

<a name=3D"L0388"></a>;; <b>TRAILER</b>
L0388:  LD      B,$1A           ; counter to twenty six.

<a name=3D"L038A"></a>;; <b>COUNTER</b>
L038A:  DEC     E               ; decrement the measuring timer.
        IN      A,($FE)         ; read the
        RLA                     ;
        BIT     7,E             ;
        LD      A,E             ;
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0388">L0388</a>         ; loop back with carry to TRAILER

        DJNZ    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L038A">L038A</a>           ; to COUNTER

        POP     DE              ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L039C">L039C</a>        ; to BIT-DONE

        CP      $56             ;
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L034E">L034E</a>        ; to NEXT-BIT

<a name=3D"L039C"></a>;; <b>BIT-DONE</b>
L039C:  CCF                     ; complement carry flag
        RL      C               ;
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L034E">L034E</a>        ; to NEXT-BIT

        RET                     ; return with full byte.

; ---

; if break is pressed while loading data then perform a reset.
; if break pressed while waiting for program on tape then OK to break.

<a name=3D"L03A2"></a>;; <b>BREAK-4</b>
L03A2:  LD      A,D             ; transfer indicator to A.
        AND     A               ; test for zero.
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0361">L0361</a>         ; back if so to RESTART


<a name=3D"L03A6"></a>;; <b>REPORT-D</b>
L03A6:  RST     08H             ; ERROR-1
        DEFB    $0C             ; Error Report: BREAK - CONT repeats

; -----------------------------
; THE <b><font color=3D"#333388">'PROGRAM NAME'</font></b> SUBROUTINE
; -----------------------------
;
;

<a name=3D"L03A8"></a>;; <b>NAME</b>
L03A8:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0F55">L0F55</a>           ; routine SCANNING
        LD      A,($4001)       ; sv FLAGS
        ADD     A,A             ;
        JP      M,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0D9A">L0D9A</a>         ; to REPORT-C

        POP     HL              ;
        RET     NC              ;

        PUSH    HL              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L02E7">L02E7</a>           ; routine SET-FAST
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L13F8">L13F8</a>           ; routine STK-FETCH
        LD      H,D             ;
        LD      L,E             ;
        DEC     C               ;
        RET     M               ;

        ADD     HL,BC           ;
        SET     7,(HL)          ;
        RET                     ;

; -------------------------
; THE <b><font color=3D"#333388">'NEW'</font></b> COMMAND ROUTINE
; -------------------------
;
;

<a name=3D"L03C3"></a>;; <b>NEW</b>
L03C3:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L02E7">L02E7</a>           ; routine SET-FAST
        LD      BC,($4004)      ; fetch value of system variable RAMTOP
        DEC     BC              ; point to last system byte.

; -----------------------
; THE <b><font color=3D"#333388">'RAM CHECK'</font></b> ROUTINE
; -----------------------
;
;

<a name=3D"L03CB"></a>;; <b>RAM-CHECK</b>
L03CB:  LD      H,B             ;
        LD      L,C             ;
        LD      A,$3F           ;

<a name=3D"L03CF"></a>;; <b>RAM-FILL</b>
L03CF:  LD      (HL),$02        ;
        DEC     HL              ;
        CP      H               ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L03CF">L03CF</a>        ; to RAM-FILL

<a name=3D"L03D5"></a>;; <b>RAM-READ</b>
L03D5:  AND     A               ;
        SBC     HL,BC           ;
        ADD     HL,BC           ;
        INC     HL              ;
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L03E2">L03E2</a>        ; to SET-TOP

        DEC     (HL)            ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L03E2">L03E2</a>         ; to SET-TOP

        DEC     (HL)            ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L03D5">L03D5</a>         ; to RAM-READ

<a name=3D"L03E2"></a>;; <b>SET-TOP</b>
L03E2:  LD      ($4004),HL      ; set system variable RAMTOP to first byte=
=20
                                ; above the BASIC system area.

; ----------------------------
; THE <b><font color=3D"#333388">'INITIALIZATION'</font></b> ROUTINE
; ----------------------------
;
;

<a name=3D"L03E5"></a>;; <b>INITIAL</b>
L03E5:  LD      HL,($4004)      ; fetch system variable RAMTOP.
        DEC     HL              ; point to last system byte.
        LD      (HL),$3E        ; make GO SUB end-marker $3E - too high for
                                ; high order byte of line number.
                                ; (was $3F on ZX80)
        DEC     HL              ; point to unimportant low-order byte.
        LD      SP,HL           ; and initialize the stack-pointer to this
                                ; location.
        DEC     HL              ; point to first location on the machine st=
ack
        DEC     HL              ; which will be filled by next CALL/PUSH.
        LD      ($4002),HL      ; set the error stack pointer ERR_SP to
                                ; the base of the now empty machine stack.

; Now set the I register so that the video hardware knows where to find the
; character set. This ROM only uses the character set when printing to=20
; the ZX Printer. The TV picture is formed by the external video hardware.=
=20
; Consider also, that this 8K ROM can be retro-fitted to the ZX80 instead o=
f=20
; its original 4K ROM so the video hardware could be on the ZX80.

        LD      A,$1E           ; address for this ROM is $1E00.
        LD      I,A             ; set I register from A.
        IM      1               ; select Z80 Interrupt Mode 1.

        LD      IY,$4000        ; set IY to the start of RAM so that the=20
                                ; system variables can be indexed.
        LD      (IY+$3B),$40    ; set CDFLAG 0100 0000. Bit 6 indicates=20
                                ; Compute nad Display required.

        LD      HL,$407D        ; The first location after System Variables=
 -
                                ; 16509 decimal.
        LD      ($400C),HL      ; set system variable D_FILE to this value.
        LD      B,$19           ; prepare minimal screen of 24 NEWLINEs
                                ; following an initial NEWLINE.

<a name=3D"L0408"></a>;; <b>LINE</b>
L0408:  LD      (HL),$76        ; insert NEWLINE (HALT instruction)
        INC     HL              ; point to next location.
        DJNZ    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0408">L0408</a>           ; loop back for all twenty five to LINE

        LD      ($4010),HL      ; set system variable VARS to next location

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L149A">L149A</a>           ; routine CLEAR sets $80 end-marker and the=
=20
                                ; dynamic memory pointers E_LINE, STKBOT an=
d
                                ; STKEND.

<a name=3D"L0413"></a>;; <b>N/L-ONLY</b>
L0413:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L14AD">L14AD</a>           ; routine CURSOR-IN inserts the cursor and=
=20
                                ; end-marker in the Edit Line also setting
                                ; size of lower display to two lines.

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0207">L0207</a>           ; routine SLOW/FAST selects COMPUTE and DISP=
LAY

; ---------------------------
; THE <b><font color=3D"#333388">'BASIC LISTING'</font></b> SECTION
; ---------------------------
;
;

<a name=3D"L0419"></a>;; <b>UPPER</b>
L0419:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0A2A">L0A2A</a>           ; routine CLS
        LD      HL,($400A)      ; sv E_PPC_lo
        LD      DE,($4023)      ; sv S_TOP_lo
        AND     A               ;
        SBC     HL,DE           ;
        EX      DE,HL           ;
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L042D">L042D</a>        ; to ADDR-TOP

        ADD     HL,DE           ;
        LD      ($4023),HL      ; sv S_TOP_lo

<a name=3D"L042D"></a>;; <b>ADDR-TOP</b>
L042D:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L09D8">L09D8</a>           ; routine LINE-ADDR
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0433">L0433</a>         ; to LIST-TOP

        EX      DE,HL           ;

<a name=3D"L0433"></a>;; <b>LIST-TOP</b>
L0433:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L073E">L073E</a>           ; routine LIST-PROG
        DEC     (IY+$1E)        ; sv BERG
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0472">L0472</a>        ; to LOWER

        LD      HL,($400A)      ; sv E_PPC_lo
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L09D8">L09D8</a>           ; routine LINE-ADDR
        LD      HL,($4016)      ; sv CH_ADD_lo
        SCF                     ; Set Carry Flag
        SBC     HL,DE           ;
        LD      HL,$4023        ; sv S_TOP_lo
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0457">L0457</a>        ; to INC-LINE

        EX      DE,HL           ;
        LD      A,(HL)          ;
        INC     HL              ;
        LDI                     ;
        LD      (DE),A          ;
        JR       <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81=
.htm#L0419">L0419</a>          ; to UPPER

; ---

<a name=3D"L0454"></a>;; <b>DOWN-KEY</b>
L0454:  LD      HL,$400A        ; sv E_PPC_lo

<a name=3D"L0457"></a>;; <b>INC-LINE</b>
L0457:  LD      E,(HL)          ;
        INC     HL              ;
        LD      D,(HL)          ;
        PUSH    HL              ;
        EX      DE,HL           ;
        INC     HL              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L09D8">L09D8</a>           ; routine LINE-ADDR
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L05BB">L05BB</a>           ; routine LINE-NO
        POP     HL              ;

<a name=3D"L0464"></a>;; <b>KEY-INPUT</b>
L0464:  BIT     5,(IY+$2D)      ; sv FLAGX
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0472">L0472</a>        ; forward to LOWER

        LD      (HL),D          ;
        DEC     HL              ;
        LD      (HL),E          ;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0419">L0419</a>           ; to UPPER

; ----------------------------
; THE <b><font color=3D"#333388">'EDIT LINE COPY'</font></b> SECTION
; ----------------------------
; This routine sets the edit line to just the cursor when
; 1) There is not enough memory to edit a BASIC line.
; 2) The edit key is used during input.
; The entry point LOWER


<a name=3D"L046F"></a>;; <b>EDIT-INP</b>
L046F:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L14AD">L14AD</a>           ; routine CURSOR-IN sets cursor only edit li=
ne.

; -&gt;

<a name=3D"L0472"></a>;; <b>LOWER</b>
L0472:  LD      HL,($4014)      ; fetch edit line start from E_LINE.

<a name=3D"L0475"></a>;; <b>EACH-CHAR</b>
L0475:  LD      A,(HL)          ; fetch a character from edit line.
        CP      $7E             ; compare to the number marker.
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0482">L0482</a>        ; forward if not to END-LINE

        LD      BC,$0006        ; else six invisible bytes to be removed.
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0A60">L0A60</a>           ; routine RECLAIM-2
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0475">L0475</a>           ; back to EACH-CHAR

; ---

<a name=3D"L0482"></a>;; <b>END-LINE</b>
L0482:  CP      $76             ;
        INC     HL              ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0475">L0475</a>        ; to EACH-CHAR

<a name=3D"L0487"></a>;; <b>EDIT-LINE</b>
L0487:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0537">L0537</a>           ; routine CURSOR sets cursor K or L.

<a name=3D"L048A"></a>;; <b>EDIT-ROOM</b>
L048A:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0A1F">L0A1F</a>           ; routine LINE-ENDS
        LD      HL,($4014)      ; sv E_LINE_lo
        LD      (IY+$00),$FF    ; sv ERR_NR
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0766">L0766</a>           ; routine COPY-LINE
        BIT     7,(IY+$00)      ; sv ERR_NR
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L04C1">L04C1</a>        ; to DISPLAY-6

        LD      A,($4022)       ; sv DF_SZ
        CP      $18             ;
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L04C1">L04C1</a>        ; to DISPLAY-6

        INC     A               ;
        LD      ($4022),A       ; sv DF_SZ
        LD      B,A             ;
        LD      C,$01           ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0918">L0918</a>           ; routine LOC-ADDR
        LD      D,H             ;
        LD      E,L             ;
        LD      A,(HL)          ;

<a name=3D"L04B1"></a>;; <b>FREE-LINE</b>
L04B1:  DEC     HL              ;
        CP      (HL)            ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L04B1">L04B1</a>        ; to FREE-LINE

        INC     HL              ;
        EX      DE,HL           ;
        LD      A,($4005)       ; sv RAMTOP_hi
        CP      $4D             ;
        CALL    C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0A5D">L0A5D</a>         ; routine RECLAIM-1
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L048A">L048A</a>           ; to EDIT-ROOM

; --------------------------
; THE <b><font color=3D"#333388">'WAIT FOR KEY'</font></b> SECTION
; --------------------------
;
;

<a name=3D"L04C1"></a>;; <b>DISPLAY-6</b>
L04C1:  LD      HL,$0000        ;
        LD      ($4018),HL      ; sv X_PTR_lo

        LD      HL,$403B        ; system variable CDFLAG
        BIT     7,(HL)          ;

        CALL    Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0229">L0229</a>         ; routine DISPLAY-1

<a name=3D"L04CF"></a>;; <b>SLOW-DISP</b>
L04CF:  BIT     0,(HL)          ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L04CF">L04CF</a>         ; to SLOW-DISP

        LD      BC,($4025)      ; sv LAST_K
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0F4B">L0F4B</a>           ; routine DEBOUNCE
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L07BD">L07BD</a>           ; routine DECODE

        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0472">L0472</a>        ; back to LOWER

; -------------------------------
; THE <b><font color=3D"#333388">'KEYBOARD DECODING'</font></b> SECTION
; -------------------------------
;   The decoded key value is in E and HL points to the position in the=20
;   key table. D contains zero.

<a name=3D"L04DF"></a>;; <b>K-DECODE</b>=20
L04DF:  LD      A,($4006)       ; Fetch value of system variable MODE
        DEC     A               ; test the three values together

        JP      M,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0508">L0508</a>         ; forward, if was zero, to FETCH-2

        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L04F7">L04F7</a>        ; forward, if was 2, to FETCH-1

;   The original value was one and is now zero.

        LD      ($4006),A       ; update the system variable MODE

        DEC     E               ; reduce E to range $00 - $7F
        LD      A,E             ; place in A
        SUB     $27             ; subtract 39 setting carry if range 00 - 3=
8
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L04F2">L04F2</a>         ; forward, if so, to FUNC-BASE

        LD      E,A             ; else set E to reduced value

<a name=3D"L04F2"></a>;; <b>FUNC-BASE</b>
L04F2:  LD      HL,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L00CC">L00CC</a>        ; address of K-FUNCT table for function keys=
.
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0505">L0505</a>           ; forward to TABLE-ADD

; ---

<a name=3D"L04F7"></a>;; <b>FETCH-1</b>
L04F7:  LD      A,(HL)          ;
        CP      $76             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L052B">L052B</a>         ; to K/L-KEY

        CP      $40             ;
        SET     7,A             ;
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L051B">L051B</a>         ; to ENTER

        LD      HL,$00C7        ; (expr reqd)

<a name=3D"L0505"></a>;; <b>TABLE-ADD</b>
L0505:  ADD     HL,DE           ;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0515">L0515</a>           ; to FETCH-3

; ---

<a name=3D"L0508"></a>;; <b>FETCH-2</b>
L0508:  LD      A,(HL)          ;
        BIT     2,(IY+$01)      ; sv FLAGS  - K or L mode ?
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0516">L0516</a>        ; to TEST-CURS

        ADD     A,$C0           ;
        CP      $E6             ;
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0516">L0516</a>        ; to TEST-CURS

<a name=3D"L0515"></a>;; <b>FETCH-3</b>
L0515:  LD      A,(HL)          ;

<a name=3D"L0516"></a>;; <b>TEST-CURS</b>
L0516:  CP      $F0             ;
        JP      PE,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L052D">L052D</a>        ; to KEY-SORT

<a name=3D"L051B"></a>;; <b>ENTER</b>
L051B:  LD      E,A             ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0537">L0537</a>           ; routine CURSOR

        LD      A,E             ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0526">L0526</a>           ; routine ADD-CHAR

<a name=3D"L0523"></a>;; <b>BACK-NEXT</b>
L0523:  JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0472">L0472</a>           ; back to LOWER

; ------------------------------
; THE <b><font color=3D"#333388">'ADD CHARACTER'</font></b> SUBROUTINE
; ------------------------------
;
;

<a name=3D"L0526"></a>;; <b>ADD-CHAR</b>
L0526:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L099B">L099B</a>           ; routine ONE-SPACE
        LD      (DE),A          ;
        RET                     ;

; -------------------------
; THE <b><font color=3D"#333388">'CURSOR KEYS'</font></b> ROUTINE
; -------------------------
;
;

<a name=3D"L052B"></a>;; <b>K/L-KEY</b>
L052B:  LD      A,$78           ;

<a name=3D"L052D"></a>;; <b>KEY-SORT</b>
L052D:  LD      E,A             ;
        LD      HL,$0482        ; base address of ED-KEYS (exp reqd)
        ADD     HL,DE           ;
        ADD     HL,DE           ;
        LD      C,(HL)          ;
        INC     HL              ;
        LD      B,(HL)          ;
        PUSH    BC              ;

<a name=3D"L0537"></a>;; <b>CURSOR</b>
L0537:  LD      HL,($4014)      ; sv E_LINE_lo
        BIT     5,(IY+$2D)      ; sv FLAGX
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0556">L0556</a>        ; to L-MODE

<a name=3D"L0540"></a>;; <b>K-MODE</b>
L0540:  RES     2,(IY+$01)      ; sv FLAGS  - Signal use K mode

<a name=3D"L0544"></a>;; <b>TEST-CHAR</b>
L0544:  LD      A,(HL)          ;
        CP      $7F             ;
        RET     Z               ; return

        INC     HL              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L07B4">L07B4</a>           ; routine NUMBER
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0544">L0544</a>         ; to TEST-CHAR

        CP      $26             ;
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0544">L0544</a>         ; to TEST-CHAR

        CP      $DE             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0540">L0540</a>         ; to K-MODE

<a name=3D"L0556"></a>;; <b>L-MODE</b>
L0556:  SET     2,(IY+$01)      ; sv FLAGS  - Signal use L mode
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0544">L0544</a>           ; to TEST-CHAR

; --------------------------
; THE <b><font color=3D"#333388">'CLEAR-ONE'</font></b> SUBROUTINE
; --------------------------
;
;

<a name=3D"L055C"></a>;; <b>CLEAR-ONE</b>
L055C:  LD      BC,$0001        ;
        JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0A60">L0A60</a>           ; to RECLAIM-2



; ------------------------
; THE <b><font color=3D"#333388">'EDITING KEYS'</font></b> TABLE
; ------------------------
;
;

<a name=3D"L0562"></a>;; <b>ED-KEYS</b>
L0562:  DEFW    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L059F">L059F</a>           ; Address: $059F; Address: UP-KEY
        DEFW    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0454">L0454</a>           ; Address: $0454; Address: DOWN-KEY
        DEFW    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0576">L0576</a>           ; Address: $0576; Address: LEFT-KEY
        DEFW    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L057F">L057F</a>           ; Address: $057F; Address: RIGHT-KEY
        DEFW    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L05AF">L05AF</a>           ; Address: $05AF; Address: FUNCTION
        DEFW    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L05C4">L05C4</a>           ; Address: $05C4; Address: EDIT-KEY
        DEFW    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L060C">L060C</a>           ; Address: $060C; Address: N/L-KEY
        DEFW    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L058B">L058B</a>           ; Address: $058B; Address: RUBOUT
        DEFW    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L05AF">L05AF</a>           ; Address: $05AF; Address: FUNCTION
        DEFW    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L05AF">L05AF</a>           ; Address: $05AF; Address: FUNCTION


; -------------------------
; THE <b><font color=3D"#333388">'CURSOR LEFT'</font></b> ROUTINE
; -------------------------
;
;

<a name=3D"L0576"></a>;; <b>LEFT-KEY</b>
L0576:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0593">L0593</a>           ; routine LEFT-EDGE
        LD      A,(HL)          ;
        LD      (HL),$7F        ;
        INC     HL              ;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0588">L0588</a>           ; to GET-CODE

; --------------------------
; THE <b><font color=3D"#333388">'CURSOR RIGHT'</font></b> ROUTINE
; --------------------------
;
;

<a name=3D"L057F"></a>;; <b>RIGHT-KEY</b>
L057F:  INC     HL              ;
        LD      A,(HL)          ;
        CP      $76             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L059D">L059D</a>         ; to ENDED-2

        LD      (HL),$7F        ;
        DEC     HL              ;

<a name=3D"L0588"></a>;; <b>GET-CODE</b>
L0588:  LD      (HL),A          ;

<a name=3D"L0589"></a>;; <b>ENDED-1</b>
L0589:  JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0523">L0523</a>           ; to BACK-NEXT

; --------------------
; THE <b><font color=3D"#333388">'RUBOUT'</font></b> ROUTINE
; --------------------
;
;

<a name=3D"L058B"></a>;; <b>RUBOUT</b>
L058B:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0593">L0593</a>           ; routine LEFT-EDGE
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L055C">L055C</a>           ; routine CLEAR-ONE
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0589">L0589</a>           ; to ENDED-1

; ------------------------
; THE <b><font color=3D"#333388">'ED-EDGE'</font></b> SUBROUTINE
; ------------------------
;
;

<a name=3D"L0593"></a>;; <b>LEFT-EDGE</b>
L0593:  DEC     HL              ;
        LD      DE,($4014)      ; sv E_LINE_lo
        LD      A,(DE)          ;
        CP      $7F             ;
        RET     NZ              ;

        POP     DE              ;

<a name=3D"L059D"></a>;; <b>ENDED-2</b>
L059D:  JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0589">L0589</a>           ; to ENDED-1

; -----------------------
; THE <b><font color=3D"#333388">'CURSOR UP'</font></b> ROUTINE
; -----------------------
;
;

<a name=3D"L059F"></a>;; <b>UP-KEY</b>
L059F:  LD      HL,($400A)      ; sv E_PPC_lo
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L09D8">L09D8</a>           ; routine LINE-ADDR
        EX      DE,HL           ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L05BB">L05BB</a>           ; routine LINE-NO
        LD      HL,$400B        ; point to system variable E_PPC_hi
        JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0464">L0464</a>           ; jump back to KEY-INPUT

; --------------------------
; THE <b><font color=3D"#333388">'FUNCTION KEY'</font></b> ROUTINE
; --------------------------
;
;

<a name=3D"L05AF"></a>;; <b>FUNCTION</b>
L05AF:  LD      A,E             ;
        AND     $07             ;
        LD      ($4006),A       ; sv MODE
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L059D">L059D</a>           ; back to ENDED-2

; ------------------------------------
; THE <b><font color=3D"#333388">'COLLECT LINE NUMBER'</font></b> SUBROUTIN=
E
; ------------------------------------
;
;

<a name=3D"L05B7"></a>;; <b>ZERO-DE</b>
L05B7:  EX      DE,HL           ;
        LD      DE,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L04C1">L04C1</a> + 1    ; $04C2 - a location addressing two zeros.

; -&gt;

<a name=3D"L05BB"></a>;; <b>LINE-NO</b>
L05BB:  LD      A,(HL)          ;
        AND     $C0             ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L05B7">L05B7</a>        ; to ZERO-DE

        LD      D,(HL)          ;
        INC     HL              ;
        LD      E,(HL)          ;
        RET                     ;

; ----------------------
; THE <b><font color=3D"#333388">'EDIT KEY'</font></b> ROUTINE
; ----------------------
;
;

<a name=3D"L05C4"></a>;; <b>EDIT-KEY</b>
L05C4:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0A1F">L0A1F</a>           ; routine LINE-ENDS clears lower display.

        LD      HL,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L046F">L046F</a>        ; Address: EDIT-INP
        PUSH    HL              ; ** is pushed as an error looping address.

        BIT     5,(IY+$2D)      ; test FLAGX
        RET     NZ              ; indirect jump if in input mode
                                ; to <a href=3D"https://www.tablix.org/~avi=
an/spectrum/rom/zx81.htm#L046F">L046F</a>, EDIT-INP (begin again).

;

        LD      HL,($4014)      ; fetch E_LINE
        LD      ($400E),HL      ; and use to update the screen cursor DF_CC

; so now RST $10 will print the line numbers to the edit line instead of sc=
reen.
; first make sure that no newline/out of screen can occur while sprinting t=
he
; line numbers to the edit line.

        LD      HL,$1821        ; prepare line 0, column 0.
        LD      ($4039),HL      ; update S_POSN with these dummy values.

        LD      HL,($400A)      ; fetch current line from E_PPC may be a=20
                                ; non-existent line e.g. last line deleted.
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L09D8">L09D8</a>           ; routine LINE-ADDR gets address or that of
                                ; the following line.
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L05BB">L05BB</a>           ; routine LINE-NO gets line number if any in=
 DE
                                ; leaving HL pointing at second low byte.

        LD      A,D             ; test the line number for zero.
        OR      E               ;
        RET     Z               ; return if no line number - no program to =
edit.

        DEC     HL              ; point to high byte.
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0AA5">L0AA5</a>           ; routine OUT-NO writes number to edit line.

        INC     HL              ; point to length bytes.
        LD      C,(HL)          ; low byte to C.
        INC     HL              ;
        LD      B,(HL)          ; high byte to B.

        INC     HL              ; point to first character in line.
        LD      DE,($400E)      ; fetch display file cursor DF_CC

        LD      A,$7F           ; prepare the cursor character.
        LD      (DE),A          ; and insert in edit line.
        INC     DE              ; increment intended destination.

        PUSH    HL              ; * save start of BASIC.

        LD      HL,$001D        ; set an overhead of 29 bytes.
        ADD     HL,DE           ; add in the address of cursor.
        ADD     HL,BC           ; add the length of the line.
        SBC     HL,SP           ; subtract the stack pointer.

        POP     HL              ; * restore pointer to start of BASIC.

        RET     NC              ; return if not enough room to L046F EDIT-I=
NP.
                                ; the edit key appears not to work.

        LDIR                    ; else copy bytes from program to edit line=
.
                                ; <font color=3D"#9900FF">Note.</font> hidd=
en floating point forms are also
                                ; copied to edit line.

        EX      DE,HL           ; transfer free location pointer to HL

        POP     DE              ; ** remove address EDIT-INP from stack.

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L14A6">L14A6</a>           ; routine SET-STK-B sets STKEND from HL.

        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L059D">L059D</a>           ; back to ENDED-2 and after 3 more jumps
                                ; to <a href=3D"https://www.tablix.org/~avi=
an/spectrum/rom/zx81.htm#L0472">L0472</a>, LOWER.
                                ; <font color=3D"#9900FF">Note.</font> The =
LOWER routine removes the hidden=20
                                ; floating-point numbers from the edit line=
.

; -------------------------
; THE <b><font color=3D"#333388">'NEWLINE KEY'</font></b> ROUTINE
; -------------------------
;
;

<a name=3D"L060C"></a>;; <b>N/L-KEY</b>
L060C:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0A1F">L0A1F</a>           ; routine LINE-ENDS

        LD      HL,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0472">L0472</a>        ; prepare address: LOWER

        BIT     5,(IY+$2D)      ; sv FLAGX
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0629">L0629</a>        ; to NOW-SCAN

        LD      HL,($4014)      ; sv E_LINE_lo
        LD      A,(HL)          ;
        CP      $FF             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0626">L0626</a>         ; to STK-UPPER

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L08E2">L08E2</a>           ; routine CLEAR-PRB
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0A2A">L0A2A</a>           ; routine CLS

<a name=3D"L0626"></a>;; <b>STK-UPPER</b>
L0626:  LD      HL,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0419">L0419</a>        ; Address: UPPER

<a name=3D"L0629"></a>;; <b>NOW-SCAN</b>
L0629:  PUSH    HL              ; push routine address (LOWER or UPPER).
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0CBA">L0CBA</a>           ; routine LINE-SCAN
        POP     HL              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0537">L0537</a>           ; routine CURSOR
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L055C">L055C</a>           ; routine CLEAR-ONE
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0A73">L0A73</a>           ; routine E-LINE-NO
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L064E">L064E</a>        ; to N/L-INP

        LD      A,B             ;
        OR      C               ;
        JP      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L06E0">L06E0</a>        ; to N/L-LINE

        DEC     BC              ;
        DEC     BC              ;
        LD      ($4007),BC      ; sv PPC_lo
        LD      (IY+$22),$02    ; sv DF_SZ
        LD      DE,($400C)      ; sv D_FILE_lo

        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0661">L0661</a>           ; forward to TEST-NULL

; ---

<a name=3D"L064E"></a>;; <b>N/L-INP</b>
L064E:  CP      $76             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0664">L0664</a>         ; to N/L-NULL

        LD      BC,($4030)      ; sv T_ADDR_lo
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0918">L0918</a>           ; routine LOC-ADDR
        LD      DE,($4029)      ; sv NXTLIN_lo
        LD      (IY+$22),$02    ; sv DF_SZ

<a name=3D"L0661"></a>;; <b>TEST-NULL</b>
L0661:  RST     18H             ; GET-CHAR
        CP      $76             ;

<a name=3D"L0664"></a>;; <b>N/L-NULL</b>
L0664:  JP      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0413">L0413</a>         ; to N/L-ONLY

        LD      (IY+$01),$80    ; sv FLAGS
        EX      DE,HL           ;

<a name=3D"L066C"></a>;; <b>NEXT-LINE</b>
L066C:  LD      ($4029),HL      ; sv NXTLIN_lo
        EX      DE,HL           ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L004D">L004D</a>           ; routine TEMP-PTR-2
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0CC1">L0CC1</a>           ; routine LINE-RUN
        RES     1,(IY+$01)      ; sv FLAGS  - Signal printer not in use
        LD      A,$C0           ;
        LD      (IY+$19),A      ; sv X_PTR_lo
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L14A3">L14A3</a>           ; routine X-TEMP
        RES     5,(IY+$2D)      ; sv FLAGX
        BIT     7,(IY+$00)      ; sv ERR_NR
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L06AE">L06AE</a>         ; to STOP-LINE

        LD      HL,($4029)      ; sv NXTLIN_lo
        AND     (HL)            ;
        JR       NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/z=
x81.htm#L06AE">L06AE</a>       ; to STOP-LINE

        LD      D,(HL)          ;
        INC     HL              ;
        LD      E,(HL)          ;
        LD      ($4007),DE      ; sv PPC_lo
        INC     HL              ;
        LD      E,(HL)          ;
        INC     HL              ;
        LD      D,(HL)          ;
        INC     HL              ;
        EX      DE,HL           ;
        ADD     HL,DE           ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0F46">L0F46</a>           ; routine BREAK-1
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L066C">L066C</a>         ; to NEXT-LINE

        LD      HL,$4000        ; sv ERR_NR
        BIT     7,(HL)          ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L06AE">L06AE</a>         ; to STOP-LINE

        LD      (HL),$0C        ;

<a name=3D"L06AE"></a>;; <b>STOP-LINE</b>
L06AE:  BIT     7,(IY+$38)      ; sv PR_CC
        CALL    Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0871">L0871</a>         ; routine COPY-BUFF
        LD      BC,$0121        ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0918">L0918</a>           ; routine LOC-ADDR
        LD      A,($4000)       ; sv ERR_NR
        LD      BC,($4007)      ; sv PPC_lo
        INC     A               ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L06D1">L06D1</a>         ; to REPORT

        CP      $09             ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L06CA">L06CA</a>        ; to CONTINUE

        INC     BC              ;

<a name=3D"L06CA"></a>;; <b>CONTINUE</b>
L06CA:  LD      ($402B),BC      ; sv OLDPPC_lo
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L06D1">L06D1</a>        ; to REPORT

        DEC     BC              ;

<a name=3D"L06D1"></a>;; <b>REPORT</b>
L06D1:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L07EB">L07EB</a>           ; routine OUT-CODE
        LD      A,$18           ;

        RST     10H             ; PRINT-A
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0A98">L0A98</a>           ; routine OUT-NUM
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L14AD">L14AD</a>           ; routine CURSOR-IN
        JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L04C1">L04C1</a>           ; to DISPLAY-6

; ---

<a name=3D"L06E0"></a>;; <b>N/L-LINE</b>
L06E0:  LD      ($400A),BC      ; sv E_PPC_lo
        LD      HL,($4016)      ; sv CH_ADD_lo
        EX      DE,HL           ;
        LD      HL,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0413">L0413</a>        ; Address: N/L-ONLY
        PUSH    HL              ;
        LD      HL,($401A)      ; sv STKBOT_lo
        SBC     HL,DE           ;
        PUSH    HL              ;
        PUSH    BC              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L02E7">L02E7</a>           ; routine SET-FAST
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0A2A">L0A2A</a>           ; routine CLS
        POP     HL              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L09D8">L09D8</a>           ; routine LINE-ADDR
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0705">L0705</a>        ; to COPY-OVER

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L09F2">L09F2</a>           ; routine NEXT-ONE
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0A60">L0A60</a>           ; routine RECLAIM-2

<a name=3D"L0705"></a>;; <b>COPY-OVER</b>
L0705:  POP     BC              ;
        LD      A,C             ;
        DEC     A               ;
        OR      B               ;
        RET     Z               ;

        PUSH    BC              ;
        INC     BC              ;
        INC     BC              ;
        INC     BC              ;
        INC     BC              ;
        DEC     HL              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L099E">L099E</a>           ; routine MAKE-ROOM
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0207">L0207</a>           ; routine SLOW/FAST
        POP     BC              ;
        PUSH    BC              ;
        INC     DE              ;
        LD      HL,($401A)      ; sv STKBOT_lo
        DEC     HL              ;
        LDDR                    ; copy bytes
        LD      HL,($400A)      ; sv E_PPC_lo
        EX      DE,HL           ;
        POP     BC              ;
        LD      (HL),B          ;
        DEC     HL              ;
        LD      (HL),C          ;
        DEC     HL              ;
        LD      (HL),E          ;
        DEC     HL              ;
        LD      (HL),D          ;

        RET                     ; return.

; ---------------------------------------
; THE <b><font color=3D"#333388">'LIST'</font></b> AND 'LLIST' COMMAND ROUT=
INES
; ---------------------------------------
;
;

<a name=3D"L072C"></a>;; <b>LLIST</b>
L072C:  SET     1,(IY+$01)      ; sv FLAGS  - signal printer in use

<a name=3D"L0730"></a>;; <b>LIST</b>
L0730:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0EA7">L0EA7</a>           ; routine FIND-INT

        LD      A,B             ; fetch high byte of user-supplied line num=
ber.
        AND     $3F             ; and crudely limit to range 1-16383.

        LD      H,A             ;
        LD      L,C             ;
        LD      ($400A),HL      ; sv E_PPC_lo
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L09D8">L09D8</a>           ; routine LINE-ADDR

<a name=3D"L073E"></a>;; <b>LIST-PROG</b>
L073E:  LD      E,$00           ;

<a name=3D"L0740"></a>;; <b>UNTIL-END</b>
L0740:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0745">L0745</a>           ; routine OUT-LINE lists one line of BASIC
                                ; making an early return when the screen is
                                ; full or the end of program is reached.   =
 &gt;&gt;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0740">L0740</a>           ; loop back to UNTIL-END

; -----------------------------------
; THE <b><font color=3D"#333388">'PRINT A BASIC LINE'</font></b> SUBROUTINE
; -----------------------------------
;
;

<a name=3D"L0745"></a>;; <b>OUT-LINE</b>
L0745:  LD      BC,($400A)      ; sv E_PPC_lo
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L09EA">L09EA</a>           ; routine CP-LINES
        LD      D,$92           ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0755">L0755</a>         ; to TEST-END

        LD      DE,$0000        ;
        RL      E               ;

<a name=3D"L0755"></a>;; <b>TEST-END</b>
L0755:  LD      (IY+$1E),E      ; sv BERG
        LD      A,(HL)          ;
        CP      $40             ;
        POP     BC              ;
        RET     NC              ;

        PUSH    BC              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0AA5">L0AA5</a>           ; routine OUT-NO
        INC     HL              ;
        LD      A,D             ;

        RST     10H             ; PRINT-A
        INC     HL              ;
        INC     HL              ;

<a name=3D"L0766"></a>;; <b>COPY-LINE</b>
L0766:  LD      ($4016),HL      ; sv CH_ADD_lo
        SET     0,(IY+$01)      ; sv FLAGS  - Suppress leading space

<a name=3D"L076D"></a>;; <b>MORE-LINE</b>
L076D:  LD      BC,($4018)      ; sv X_PTR_lo
        LD      HL,($4016)      ; sv CH_ADD_lo
        AND      A              ;
        SBC     HL,BC           ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L077C">L077C</a>        ; to TEST-NUM

        LD      A,$B8           ;

        RST     10H             ; PRINT-A

<a name=3D"L077C"></a>;; <b>TEST-NUM</b>
L077C:  LD      HL,($4016)      ; sv CH_ADD_lo
        LD      A,(HL)          ;
        INC     HL              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L07B4">L07B4</a>           ; routine NUMBER
        LD      ($4016),HL      ; sv CH_ADD_lo
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L076D">L076D</a>         ; to MORE-LINE

        CP      $7F             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L079D">L079D</a>         ; to OUT-CURS

        CP      $76             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L07EE">L07EE</a>         ; to OUT-CH

        BIT     6,A             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L079A">L079A</a>         ; to NOT-TOKEN

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L094B">L094B</a>           ; routine TOKENS
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L076D">L076D</a>           ; to MORE-LINE

; ---


<a name=3D"L079A"></a>;; <b>NOT-TOKEN</b>
L079A:  RST     10H             ; PRINT-A
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L076D">L076D</a>           ; to MORE-LINE

; ---

<a name=3D"L079D"></a>;; <b>OUT-CURS</b>
L079D:  LD      A,($4006)       ; Fetch value of system variable MODE
        LD      B,$AB           ; Prepare an inverse [F] for function curso=
r.

        AND     A               ; Test for zero -
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L07AA">L07AA</a>        ; forward if not to FLAGS-2

        LD      A,($4001)       ; Fetch system variable FLAGS.
        LD      B,$B0           ; Prepare an inverse [K] for keyword cursor=
.

<a name=3D"L07AA"></a>;; <b>FLAGS-2</b>
L07AA:  RRA                     ; 00000?00 -&gt; 000000?0
        RRA                     ; 000000?0 -&gt; 0000000?
        AND     $01             ; 0000000?    0000000x

        ADD     A,B             ; Possibly [F] -&gt; [G]  or  [K] -&gt; [L]

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L07F5">L07F5</a>           ; routine PRINT-SP prints character=20
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L076D">L076D</a>           ; back to MORE-LINE

; -----------------------
; THE <b><font color=3D"#333388">'NUMBER'</font></b> SUBROUTINE
; -----------------------
;
;

<a name=3D"L07B4"></a>;; <b>NUMBER</b>
L07B4:  CP      $7E             ;
        RET     NZ              ;

        INC     HL              ;
        INC     HL              ;
        INC     HL              ;
        INC     HL              ;
        INC     HL              ;
        RET                     ;

; --------------------------------
; THE <b><font color=3D"#333388">'KEYBOARD DECODE'</font></b> SUBROUTINE
; --------------------------------
;
;

<a name=3D"L07BD"></a>;; <b>DECODE</b>
L07BD:  LD      D,$00           ;
        SRA     B               ;
        SBC     A,A             ;
        OR      $26             ;
        LD      L,$05           ;
        SUB     L               ;

<a name=3D"L07C7"></a>;; <b>KEY-LINE</b>
L07C7:  ADD     A,L             ;
        SCF                     ; Set Carry Flag
        RR      C               ;
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L07C7">L07C7</a>         ; to KEY-LINE

        INC     C               ;
        RET      NZ             ;

        LD      C,B             ;
        DEC     L               ;
        LD      L,$01           ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L07C7">L07C7</a>        ; to KEY-LINE

        LD      HL,$007D        ; (expr reqd)
        LD      E,A             ;
        ADD     HL,DE           ;
        SCF                     ; Set Carry Flag
        RET                     ;

; -------------------------
; THE <b><font color=3D"#333388">'PRINTING'</font></b> SUBROUTINE
; -------------------------
;
;

<a name=3D"L07DC"></a>;; <b>LEAD-SP</b>
L07DC:  LD      A,E             ;
        AND     A               ;
        RET     M               ;

        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L07F1">L07F1</a>           ; to PRINT-CH

; ---

<a name=3D"L07E1"></a>;; <b>OUT-DIGIT</b>
L07E1:  XOR     A               ;

<a name=3D"L07E2"></a>;; <b>DIGIT-INC</b>
L07E2:  ADD     HL,BC           ;
        INC     A               ;
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L07E2">L07E2</a>         ; to DIGIT-INC

        SBC     HL,BC           ;
        DEC     A               ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L07DC">L07DC</a>         ; to LEAD-SP

<a name=3D"L07EB"></a>;; <b>OUT-CODE</b>
L07EB:  LD      E,$1C           ;
        ADD     A,E             ;

<a name=3D"L07EE"></a>;; <b>OUT-CH</b>
L07EE:  AND     A               ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L07F5">L07F5</a>         ; to PRINT-SP

<a name=3D"L07F1"></a>;; <b>PRINT-CH</b>
L07F1:  RES     0,(IY+$01)      ; update FLAGS - signal leading space permi=
tted

<a name=3D"L07F5"></a>;; <b>PRINT-SP</b>
L07F5:  EXX                     ;
        PUSH    HL              ;
        BIT     1,(IY+$01)      ; test FLAGS - is printer in use ?
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0802">L0802</a>        ; to LPRINT-A

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0808">L0808</a>           ; routine ENTER-CH
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0805">L0805</a>           ; to PRINT-EXX

; ---

<a name=3D"L0802"></a>;; <b>LPRINT-A</b>
L0802:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0851">L0851</a>           ; routine LPRINT-CH

<a name=3D"L0805"></a>;; <b>PRINT-EXX</b>
L0805:  POP     HL              ;
        EXX                     ;
        RET                     ;

; ---

<a name=3D"L0808"></a>;; <b>ENTER-CH</b>
L0808:  LD      D,A             ;
        LD      BC,($4039)      ; sv S_POSN_x
        LD      A,C             ;
        CP      $21             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L082C">L082C</a>         ; to TEST-LOW

<a name=3D"L0812"></a>;; <b>TEST-N/L</b>
L0812:  LD      A,$76           ;
        CP      D               ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0847">L0847</a>         ; to WRITE-N/L

        LD      HL,($400E)      ; sv DF_CC_lo
        CP      (HL)            ;
        LD      A,D             ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L083E">L083E</a>        ; to WRITE-CH

        DEC     C               ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L083A">L083A</a>        ; to EXPAND-1

        INC     HL              ;
        LD       ($400E),HL     ; sv DF_CC_lo
        LD      C,$21           ;
        DEC     B               ;
        LD      ($4039),BC      ; sv S_POSN_x

<a name=3D"L082C"></a>;; <b>TEST-LOW</b>
L082C:  LD      A,B             ;
        CP      (IY+$22)        ; sv DF_SZ
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0835">L0835</a>         ; to REPORT-5

        AND     A               ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0812">L0812</a>        ; to TEST-N/L

<a name=3D"L0835"></a>;; <b>REPORT-5</b>
L0835:  LD      L,$04           ; 'No more room on screen'
        JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0058">L0058</a>           ; to ERROR-3

; ---

<a name=3D"L083A"></a>;; <b>EXPAND-1</b>
L083A:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L099B">L099B</a>           ; routine ONE-SPACE
        EX      DE,HL           ;

<a name=3D"L083E"></a>;; <b>WRITE-CH</b>
L083E:  LD      (HL),A          ;
        INC     HL              ;
        LD      ($400E),HL      ; sv DF_CC_lo
        DEC     (IY+$39)        ; sv S_POSN_x
        RET                     ;

; ---

<a name=3D"L0847"></a>;; <b>WRITE-N/L</b>
L0847:  LD      C,$21           ;
        DEC     B               ;
        SET     0,(IY+$01)      ; sv FLAGS  - Suppress leading space
        JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0918">L0918</a>           ; to LOC-ADDR

; --------------------------
; THE <b><font color=3D"#333388">'LPRINT-CH'</font></b> SUBROUTINE
; --------------------------
; This routine sends a character to the ZX-Printer placing the code for the
; character in the Printer Buffer.
; <font color=3D"#9900FF">Note.</font> PR-CC contains the low byte of the b=
uffer address. The high order byte=20
; is always constant.=20


<a name=3D"L0851"></a>;; <b>LPRINT-CH</b>
L0851:  CP      $76             ; compare to NEWLINE.
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0871">L0871</a>         ; forward if so to COPY-BUFF

        LD      C,A             ; take a copy of the character in C.
        LD      A,($4038)       ; fetch print location from PR_CC
        AND     $7F             ; ignore bit 7 to form true position.
        CP      $5C             ; compare to 33rd location

        LD      L,A             ; form low-order byte.
        LD      H,$40           ; the high-order byte is fixed.

        CALL    Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0871">L0871</a>         ; routine COPY-BUFF to send full buffer to=
=20
                                ; the printer if first 32 bytes full.
                                ; (this will reset HL to start.)

        LD      (HL),C          ; place character at location.
        INC     L               ; increment - will not cross a 256 boundary=
.
        LD      (IY+$38),L      ; update system variable PR_CC
                                ; automatically resetting bit 7 to show tha=
t
                                ; the buffer is not empty.
        RET                     ; return.

; --------------------------
; THE <b><font color=3D"#333388">'COPY'</font></b> COMMAND ROUTINE
; --------------------------
; The full character-mapped screen is copied to the ZX-Printer.
; All twenty-four text/graphic lines are printed.

<a name=3D"L0869"></a>;; <b>COPY</b>
L0869:  LD      D,$16           ; prepare to copy twenty four text lines.
        LD      HL,($400C)      ; set HL to start of display file from D_FI=
LE.
        INC     HL              ;=20
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0876">L0876</a>           ; forward to COPY*D

; ---

; A single character-mapped printer buffer is copied to the ZX-Printer.

<a name=3D"L0871"></a>;; <b>COPY-BUFF</b>
L0871:  LD      D,$01           ; prepare to copy a single text line.
        LD      HL,$403C        ; set HL to start of printer buffer PRBUFF.

; both paths converge here.

<a name=3D"L0876"></a>;; <b>COPY*D</b>
L0876:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L02E7">L02E7</a>           ; routine SET-FAST

        PUSH    BC              ; *** preserve BC throughout.
                                ; a pending character may be present=20
                                ; in C from LPRINT-CH

<a name=3D"L087A"></a>;; <b>COPY-LOOP</b>
L087A:  PUSH    HL              ; save first character of line pointer. (*)
        XOR     A               ; clear accumulator.
        LD      E,A             ; set pixel line count, range 0-7, to zero.

; this inner loop deals with each horizontal pixel line.

<a name=3D"L087D"></a>;; <b>COPY-TIME</b>
L087D:  OUT     ($FB),A         ; bit 2 reset starts the printer motor
                                ; with an inactive stylus - bit 7 reset.
        POP     HL              ; pick up first character of line pointer (=
*)
                                ; on inner loop.

<a name=3D"L0880"></a>;; <b>COPY-BRK</b>
L0880:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0F46">L0F46</a>           ; routine BREAK-1
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L088A">L088A</a>         ; forward with no keypress to COPY-CONT

; else A will hold 11111111 0

        RRA                     ; 0111 1111
        OUT     ($FB),A         ; stop ZX printer motor, de-activate stylus=
.

<a name=3D"L0888"></a>;; <b>REPORT-D2</b>
L0888:  RST     08H             ; ERROR-1
        DEFB    $0C             ; Error Report: BREAK - CONT repeats

; ---

<a name=3D"L088A"></a>;; <b>COPY-CONT</b>
L088A:  IN      A,($FB)         ; read from printer port.
        ADD     A,A             ; test bit 6 and 7
        JP      M,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L08DE">L08DE</a>         ; jump forward with no printer to COPY-END

        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0880">L0880</a>        ; back if stylus not in position to COPY-BRK

        PUSH    HL              ; save first character of line pointer (*)
        PUSH    DE              ; ** preserve character line and pixel line=
.

        LD      A,D             ; text line count to A?
        CP      $02             ; sets carry if last line.
        SBC     A,A             ; now $FF if last line else zero.

; now cleverly prepare a printer control mask setting bit 2 (later moved to=
 1)
; of D to slow printer for the last two pixel lines ( E =3D 6 and 7)

        AND     E               ; and with pixel line offset 0-7
        RLCA                    ; shift to left.
        AND     E               ; and again.
        LD      D,A             ; store control mask in D.

<a name=3D"L089C"></a>;; <b>COPY-NEXT</b>
L089C:  LD      C,(HL)          ; load character from screen or buffer.
        LD      A,C             ; save a copy in C for later inverse test.
        INC     HL              ; update pointer for next time.
        CP      $76             ; is character a NEWLINE ?
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L08C7">L08C7</a>         ; forward, if so, to COPY-N/L

        PUSH    HL              ; * else preserve the character pointer.

        SLA     A               ; (?) multiply by two
        ADD     A,A             ; multiply by four
        ADD     A,A             ; multiply by eight

        LD      H,$0F           ; load H with half the address of character=
 set.
        RL      H               ; now $1E or $1F (with carry)
        ADD     A,E             ; add byte offset 0-7
        LD      L,A             ; now HL addresses character source byte

        RL      C               ; test character, setting carry if inverse.
        SBC     A,A             ; accumulator now $00 if normal, $FF if inv=
erse.

        XOR     (HL)            ; combine with bit pattern at end or ROM.
        LD      C,A             ; transfer the byte to C.
        LD      B,$08           ; count eight bits to output.

<a name=3D"L08B5"></a>;; <b>COPY-BITS</b>
L08B5:  LD      A,D             ; fetch speed control mask from D.
        RLC     C               ; rotate a bit from output byte to carry.
        RRA                     ; pick up in bit 7, speed bit to bit 1
        LD      H,A             ; store aligned mask in H register.

<a name=3D"L08BA"></a>;; <b>COPY-WAIT</b>
L08BA:  IN      A,($FB)         ; read the printer port
        RRA                     ; test for alignment signal from encoder.
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L08BA">L08BA</a>        ; loop if not present to COPY-WAIT

        LD      A,H             ; control byte to A.
        OUT     ($FB),A         ; and output to printer port.
        DJNZ    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L08B5">L08B5</a>           ; loop for all eight bits to COPY-BITS

        POP     HL              ; * restore character pointer.
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L089C">L089C</a>           ; back for adjacent character line to COPY-N=
EXT

; ---

; A NEWLINE has been encountered either following a text line or as the=20
; first character of the screen or printer line.

<a name=3D"L08C7"></a>;; <b>COPY-N/L</b>
L08C7:  IN      A,($FB)         ; read printer port.
        RRA                     ; wait for encoder signal.
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L08C7">L08C7</a>        ; loop back if not to COPY-N/L

        LD      A,D             ; transfer speed mask to A.
        RRCA                    ; rotate speed bit to bit 1.=20
                                ; bit 7, stylus control is reset.
        OUT     ($FB),A         ; set the printer speed.

        POP     DE              ; ** restore character line and pixel line.
        INC     E               ; increment pixel line 0-7.
        BIT     3,E             ; test if value eight reached.
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L087D">L087D</a>         ; back if not to COPY-TIME

; eight pixel lines, a text line have been completed.

        POP     BC              ; lose the now redundant first character=20
                                ; pointer
        DEC     D               ; decrease text line count.
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L087A">L087A</a>        ; back if not zero to COPY-LOOP

        LD      A,$04           ; stop the already slowed printer motor.
        OUT     ($FB),A         ; output to printer port.

<a name=3D"L08DE"></a>;; <b>COPY-END</b>
L08DE:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0207">L0207</a>           ; routine SLOW/FAST
        POP     BC              ; *** restore preserved BC.

; -------------------------------------
; THE <b><font color=3D"#333388">'CLEAR PRINTER BUFFER'</font></b> SUBROUTI=
NE
; -------------------------------------
; This subroutine sets 32 bytes of the printer buffer to zero (space) and
; the 33rd character is set to a NEWLINE.
; This occurs after the printer buffer is sent to the printer but in additi=
on
; after the 24 lines of the screen are sent to the printer.=20
; <font color=3D"#9900FF">Note.</font> This is a logic error as the last op=
eration does not involve the=20
; buffer at all. Logically one should be able to use=20
; 10 LPRINT "HELLO ";
; 20 COPY
; 30 LPRINT ; "WORLD"
; and expect to see the entire greeting emerge from the printer.
; Surprisingly this logic error was never discovered and although one can a=
rgue
; if the above is a bug, the repetition of this error on the Spectrum was m=
ost
; definitely a bug.
; Since the printer buffer is fixed at the end of the system variables, and
; the print position is in the range $3C - $5C, then bit 7 of the system
; variable is set to show the buffer is empty and automatically reset when
; the variable is updated with any print position - neat.

<a name=3D"L08E2"></a>;; <b>CLEAR-PRB</b>
L08E2:  LD      HL,$405C        ; address fixed end of PRBUFF
        LD      (HL),$76        ; place a newline at last position.
        LD      B,$20           ; prepare to blank 32 preceding characters.=
=20

<a name=3D"L08E9"></a>;; <b>PRB-BYTES</b>
L08E9:  DEC     HL              ; decrement address - could be DEC L.
        LD      (HL),$00        ; place a zero byte.
        DJNZ    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L08E9">L08E9</a>           ; loop for all thirty-two to PRB-BYTES

        LD      A,L             ; fetch character print position.
        SET     7,A             ; signal the printer buffer is clear.
        LD      ($4038),A       ; update one-byte system variable PR_CC
        RET                     ; return.

; -------------------------
; THE <b><font color=3D"#333388">'PRINT AT'</font></b> SUBROUTINE
; -------------------------
;
;

<a name=3D"L08F5"></a>;; <b>PRINT-AT</b>
L08F5:  LD      A,$17           ;
        SUB     B               ;
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0905">L0905</a>         ; to WRONG-VAL

<a name=3D"L08FA"></a>;; <b>TEST-VAL</b>
L08FA:  CP      (IY+$22)        ; sv DF_SZ
        JP      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0835">L0835</a>         ; to REPORT-5

        INC     A               ;
        LD      B,A             ;
        LD      A,$1F           ;
        SUB     C               ;

<a name=3D"L0905"></a>;; <b>WRONG-VAL</b>
L0905:  JP      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0EAD">L0EAD</a>         ; to REPORT-B

        ADD     A,$02           ;
        LD      C,A             ;

<a name=3D"L090B"></a>;; <b>SET-FIELD</b>
L090B:  BIT     1,(IY+$01)      ; sv FLAGS  - Is printer in use
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0918">L0918</a>         ; to LOC-ADDR

        LD      A,$5D           ;
        SUB     C               ;
        LD      ($4038),A       ; sv PR_CC
        RET                     ;

; ----------------------------
; THE <b><font color=3D"#333388">'LOCATE ADDRESS'</font></b> ROUTINE
; ----------------------------
;
;

<a name=3D"L0918"></a>;; <b>LOC-ADDR</b>
L0918:  LD      ($4039),BC      ; sv S_POSN_x
        LD      HL,($4010)      ; sv VARS_lo
        LD      D,C             ;
        LD      A,$22           ;
        SUB     C               ;
        LD      C,A             ;
        LD      A,$76           ;
        INC     B               ;

<a name=3D"L0927"></a>;; <b>LOOK-BACK</b>
L0927:  DEC     HL              ;
        CP      (HL)            ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0927">L0927</a>        ; to LOOK-BACK

        DJNZ    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0927">L0927</a>           ; to LOOK-BACK

        INC     HL              ;
        CPIR                    ;
        DEC     HL              ;
        LD      ($400E),HL      ; sv DF_CC_lo
        SCF                     ; Set Carry Flag
        RET     PO              ;

        DEC     D               ;
        RET     Z               ;

        PUSH    BC              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L099E">L099E</a>           ; routine MAKE-ROOM
        POP     BC              ;
        LD      B,C             ;
        LD      H,D             ;
        LD       L,E            ;

<a name=3D"L0940"></a>;; <b>EXPAND-2</b>
L0940:  LD      (HL),$00        ;
        DEC     HL              ;
        DJNZ    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0940">L0940</a>           ; to EXPAND-2

        EX      DE,HL           ;
        INC     HL              ;
        LD      ($400E),HL      ; sv DF_CC_lo
        RET                     ;

; ------------------------------
; THE <b><font color=3D"#333388">'EXPAND TOKENS'</font></b> SUBROUTINE
; ------------------------------
;
;

<a name=3D"L094B"></a>;; <b>TOKENS</b>
L094B:  PUSH    AF              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0975">L0975</a>           ; routine TOKEN-ADD
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0959">L0959</a>        ; to ALL-CHARS

        BIT     0,(IY+$01)      ; sv FLAGS  - Leading space if set
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0959">L0959</a>        ; to ALL-CHARS

        XOR     A               ;

        RST     10H             ; PRINT-A

<a name=3D"L0959"></a>;; <b>ALL-CHARS</b>
L0959:  LD      A,(BC)          ;
        AND     $3F             ;

        RST     10H             ; PRINT-A
        LD      A,(BC)          ;
        INC     BC              ;
        ADD     A,A             ;
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0959">L0959</a>        ; to ALL-CHARS

        POP     BC              ;
        BIT     7,B             ;
        RET     Z               ;

        CP      $1A             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L096D">L096D</a>         ; to TRAIL-SP

        CP      $38             ;
        RET     C               ;

<a name=3D"L096D"></a>;; <b>TRAIL-SP</b>
L096D:  XOR     A               ;
        SET     0,(IY+$01)      ; sv FLAGS  - Suppress leading space
        JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L07F5">L07F5</a>           ; to PRINT-SP

; ---

<a name=3D"L0975"></a>;; <b>TOKEN-ADD</b>
L0975:  PUSH    HL              ;
        LD      HL,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0111">L0111</a>        ; Address of TOKENS
        BIT     7,A             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L097F">L097F</a>         ; to TEST-HIGH

        AND     $3F             ;

<a name=3D"L097F"></a>;; <b>TEST-HIGH</b>
L097F:  CP      $43             ;
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0993">L0993</a>        ; to FOUND

        LD      B,A             ;
        INC     B               ;

<a name=3D"L0985"></a>;; <b>WORDS</b>
L0985:  BIT     7,(HL)          ;
        INC     HL              ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0985">L0985</a>         ; to WORDS

        DJNZ    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0985">L0985</a>           ; to WORDS

        BIT     6,A             ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0992">L0992</a>        ; to COMP-FLAG

        CP      $18             ;

<a name=3D"L0992"></a>;; <b>COMP-FLAG</b>
L0992:  CCF                     ; Complement Carry Flag

<a name=3D"L0993"></a>;; <b>FOUND</b>
L0993:  LD      B,H             ;
        LD       C,L            ;
        POP     HL              ;
        RET     NC              ;

        LD      A,(BC)          ;
        ADD     A,$E4           ;
        RET                     ;

; --------------------------
; THE <b><font color=3D"#333388">'ONE SPACE'</font></b> SUBROUTINE
; --------------------------
;
;

<a name=3D"L099B"></a>;; <b>ONE-SPACE</b>
L099B:  LD      BC,$0001        ;

; --------------------------
; THE <b><font color=3D"#333388">'MAKE ROOM'</font></b> SUBROUTINE
; --------------------------
;
;

<a name=3D"L099E"></a>;; <b>MAKE-ROOM</b>
L099E:  PUSH    HL              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0EC5">L0EC5</a>           ; routine TEST-ROOM
        POP     HL              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L09AD">L09AD</a>           ; routine POINTERS
        LD      HL,($401C)      ; sv STKEND_lo
        EX      DE,HL           ;
        LDDR                    ; Copy Bytes
        RET                     ;

; -------------------------
; THE <b><font color=3D"#333388">'POINTERS'</font></b> SUBROUTINE
; -------------------------
;
;

<a name=3D"L09AD"></a>;; <b>POINTERS</b>
L09AD:  PUSH    AF              ;
        PUSH    HL              ;
        LD      HL,$400C        ; sv D_FILE_lo
        LD      A,$09           ;

<a name=3D"L09B4"></a>;; <b>NEXT-PTR</b>
L09B4:  LD      E,(HL)          ;
        INC     HL              ;
        LD      D,(HL)          ;
        EX      (SP),HL         ;
        AND     A               ;
        SBC     HL,DE           ;
        ADD     HL,DE           ;
        EX      (SP),HL         ;
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L09C8">L09C8</a>        ; to PTR-DONE

        PUSH    DE              ;
        EX      DE,HL           ;
        ADD     HL,BC           ;
        EX      DE,HL           ;
        LD      (HL),D          ;
        DEC     HL              ;
        LD      (HL),E          ;
        INC     HL              ;
        POP     DE              ;

<a name=3D"L09C8"></a>;; <b>PTR-DONE</b>
L09C8:  INC     HL              ;
        DEC     A               ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L09B4">L09B4</a>        ; to NEXT-PTR

        EX      DE,HL           ;
        POP     DE              ;
        POP     AF              ;
        AND     A               ;
        SBC     HL,DE           ;
        LD      B,H             ;
        LD      C,L             ;
        INC     BC              ;
        ADD     HL,DE           ;
        EX      DE,HL           ;
        RET                     ;

; -----------------------------
; THE <b><font color=3D"#333388">'LINE ADDRESS'</font></b> SUBROUTINE
; -----------------------------
;
;

<a name=3D"L09D8"></a>;; <b>LINE-ADDR</b>
L09D8:  PUSH    HL              ;
        LD      HL,$407D        ;
        LD      D,H             ;
        LD      E,L             ;

<a name=3D"L09DE"></a>;; <b>NEXT-TEST</b>
L09DE:  POP     BC              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L09EA">L09EA</a>           ; routine CP-LINES
        RET     NC              ;

        PUSH    BC              ;
        CALL     <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81=
.htm#L09F2">L09F2</a>          ; routine NEXT-ONE
        EX      DE,HL           ;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L09DE">L09DE</a>           ; to NEXT-TEST

; -------------------------------------
; THE <b><font color=3D"#333388">'COMPARE LINE NUMBERS'</font></b> SUBROUTI=
NE
; -------------------------------------
;
;

<a name=3D"L09EA"></a>;; <b>CP-LINES</b>
L09EA:  LD      A,(HL)          ;
        CP      B               ;
        RET     NZ              ;

        INC     HL              ;
        LD      A,(HL)          ;
        DEC     HL              ;
        CP      C               ;
        RET                     ;

; --------------------------------------
; THE <b><font color=3D"#333388">'NEXT LINE OR VARIABLE'</font></b> SUBROUT=
INE
; --------------------------------------
;
;

<a name=3D"L09F2"></a>;; <b>NEXT-ONE</b>
L09F2:  PUSH    HL              ;
        LD      A,(HL)          ;
        CP      $40             ;
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0A0F">L0A0F</a>         ; to LINES

        BIT     5,A             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0A10">L0A10</a>         ; forward to NEXT-O-4

        ADD     A,A             ;
        JP      M,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0A01">L0A01</a>         ; to NEXT+FIVE

        CCF                     ; Complement Carry Flag

<a name=3D"L0A01"></a>;; <b>NEXT+FIVE</b>
L0A01:  LD      BC,$0005        ;
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0A08">L0A08</a>        ; to NEXT-LETT

        LD      C,$11           ;

<a name=3D"L0A08"></a>;; <b>NEXT-LETT</b>
L0A08:  RLA                     ;
        INC     HL              ;
        LD      A,(HL)          ;
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0A08">L0A08</a>        ; to NEXT-LETT

        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0A15">L0A15</a>           ; to NEXT-ADD

; ---

<a name=3D"L0A0F"></a>;; <b>LINES</b>
L0A0F:  INC     HL              ;

<a name=3D"L0A10"></a>;; <b>NEXT-O-4</b>
L0A10:  INC     HL              ;
        LD      C,(HL)          ;
        INC     HL              ;
        LD      B,(HL)          ;
        INC     HL              ;

<a name=3D"L0A15"></a>;; <b>NEXT-ADD</b>
L0A15:  ADD     HL,BC           ;
        POP     DE              ;

; ---------------------------
; THE <b><font color=3D"#333388">'DIFFERENCE'</font></b> SUBROUTINE
; ---------------------------
;
;

<a name=3D"L0A17"></a>;; <b>DIFFER</b>
L0A17:  AND     A               ;
        SBC     HL,DE           ;
        LD      B,H             ;
        LD      C,L             ;
        ADD     HL,DE           ;
        EX      DE,HL           ;
        RET                     ;

; --------------------------
; THE <b><font color=3D"#333388">'LINE-ENDS'</font></b> SUBROUTINE
; --------------------------
;
;

<a name=3D"L0A1F"></a>;; <b>LINE-ENDS</b>
L0A1F:  LD      B,(IY+$22)      ; sv DF_SZ
        PUSH    BC              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0A2C">L0A2C</a>           ; routine B-LINES
        POP     BC              ;
        DEC     B               ;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0A2C">L0A2C</a>           ; to B-LINES

; -------------------------
; THE <b><font color=3D"#333388">'CLS'</font></b> COMMAND ROUTINE
; -------------------------
;
;

<a name=3D"L0A2A"></a>;; <b>CLS</b>
L0A2A:  LD      B,$18           ;

<a name=3D"L0A2C"></a>;; <b>B-LINES</b>
L0A2C:  RES     1,(IY+$01)      ; sv FLAGS  - Signal printer not in use
        LD      C,$21           ;
        PUSH    BC              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0918">L0918</a>           ; routine LOC-ADDR
        POP     BC              ;
        LD      A,($4005)       ; sv RAMTOP_hi
        CP      $4D             ;
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0A52">L0A52</a>         ; to COLLAPSED

        SET     7,(IY+$3A)      ; sv S_POSN_y

<a name=3D"L0A42"></a>;; <b>CLEAR-LOC</b>
L0A42:  XOR     A               ; prepare a space
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L07F5">L07F5</a>           ; routine PRINT-SP prints a space
        LD      HL,($4039)      ; sv S_POSN_x
        LD      A,L             ;
        OR      H               ;
        AND     $7E             ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0A42">L0A42</a>        ; to CLEAR-LOC

        JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0918">L0918</a>           ; to LOC-ADDR

; ---

<a name=3D"L0A52"></a>;; <b>COLLAPSED</b>
L0A52:  LD      D,H             ;
        LD      E,L             ;
        DEC     HL              ;
        LD      C,B             ;
        LD      B,$00           ;
        LDIR                    ; Copy Bytes
        LD      HL,($4010)      ; sv VARS_lo

; ----------------------------
; THE <b><font color=3D"#333388">'RECLAIMING'</font></b> SUBROUTINES
; ----------------------------
;
;

<a name=3D"L0A5D"></a>;; <b>RECLAIM-1</b>
L0A5D:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0A17">L0A17</a>           ; routine DIFFER

<a name=3D"L0A60"></a>;; <b>RECLAIM-2</b>
L0A60:  PUSH    BC              ;
        LD      A,B             ;
        CPL                     ;
        LD      B,A             ;
        LD      A,C             ;
        CPL                     ;
        LD      C,A             ;
        INC     BC              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L09AD">L09AD</a>           ; routine POINTERS
        EX      DE,HL           ;
        POP     HL              ;
        ADD     HL,DE           ;
        PUSH    DE              ;
        LDIR                    ; Copy Bytes
        POP     HL              ;
        RET                     ;

; ------------------------------
; THE <b><font color=3D"#333388">'E-LINE NUMBER'</font></b> SUBROUTINE
; ------------------------------
;
;

<a name=3D"L0A73"></a>;; <b>E-LINE-NO</b>
L0A73:  LD      HL,($4014)      ; sv E_LINE_lo
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L004D">L004D</a>           ; routine TEMP-PTR-2

        RST     18H             ; GET-CHAR
        BIT     5,(IY+$2D)      ; sv FLAGX
        RET     NZ              ;

        LD      HL,$405D        ; sv MEM-0-1st
        LD      ($401C),HL      ; sv STKEND_lo
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L1548">L1548</a>           ; routine INT-TO-FP
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L158A">L158A</a>           ; routine FP-TO-BC
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0A91">L0A91</a>         ; to NO-NUMBER

        LD      HL,$D8F0        ; value '-10000'
        ADD     HL,BC           ;

<a name=3D"L0A91"></a>;; <b>NO-NUMBER</b>
L0A91:  JP      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0D9A">L0D9A</a>         ; to REPORT-C

        CP      A               ;
        JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L14BC">L14BC</a>           ; routine SET-MIN

; -------------------------------------------------
; THE <b><font color=3D"#333388">'REPORT AND LINE NUMBER'</font></b> PRINTI=
NG SUBROUTINES
; -------------------------------------------------
;
;

<a name=3D"L0A98"></a>;; <b>OUT-NUM</b>
L0A98:  PUSH    DE              ;
        PUSH    HL              ;
        XOR     A               ;
        BIT     7,B             ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0ABF">L0ABF</a>        ; to UNITS

        LD       H,B            ;
        LD      L,C             ;
        LD      E,$FF           ;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0AAD">L0AAD</a>           ; to THOUSAND

; ---

<a name=3D"L0AA5"></a>;; <b>OUT-NO</b>
L0AA5:  PUSH    DE              ;
        LD      D,(HL)          ;
        INC     HL              ;
        LD      E,(HL)          ;
        PUSH    HL              ;
        EX      DE,HL           ;
        LD      E,$00           ; set E to leading space.

<a name=3D"L0AAD"></a>;; <b>THOUSAND</b>
L0AAD:  LD      BC,$FC18        ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L07E1">L07E1</a>           ; routine OUT-DIGIT
        LD      BC,$FF9C        ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L07E1">L07E1</a>           ; routine OUT-DIGIT
        LD      C,$F6           ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L07E1">L07E1</a>           ; routine OUT-DIGIT
        LD      A,L             ;

<a name=3D"L0ABF"></a>;; <b>UNITS</b>
L0ABF:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L07EB">L07EB</a>           ; routine OUT-CODE
        POP     HL              ;
        POP     DE              ;
        RET                     ;

; --------------------------
; THE <b><font color=3D"#333388">'UNSTACK-Z'</font></b> SUBROUTINE
; --------------------------
; This subroutine is used to return early from a routine when checking synt=
ax.
; On the ZX81 the same routines that execute commands also check the syntax
; on line entry. This enables precise placement of the error marker in a li=
ne
; that fails syntax.
; The sequence CALL SYNTAX-Z ; RET Z can be replaced by a call to this rout=
ine
; although it has not replaced every occurrence of the above two instructio=
ns.
; Even on the ZX-80 this routine was not fully utilized.

<a name=3D"L0AC5"></a>;; <b>UNSTACK-Z</b>
L0AC5:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0DA6">L0DA6</a>           ; routine SYNTAX-Z resets the ZERO flag if
                                ; checking syntax.
        POP     HL              ; drop the return address.
        RET     Z               ; return to previous calling routine if=20
                                ; checking syntax.

        JP      (HL)            ; else jump to the continuation address in
                                ; the calling routine as RET would have don=
e.

; ----------------------------
; THE <b><font color=3D"#333388">'LPRINT'</font></b> COMMAND ROUTINE
; ----------------------------
;
;

<a name=3D"L0ACB"></a>;; <b>LPRINT</b>
L0ACB:  SET     1,(IY+$01)      ; sv FLAGS  - Signal printer in use

; ---------------------------
; THE <b><font color=3D"#333388">'PRINT'</font></b> COMMAND ROUTINE
; ---------------------------
;
;

<a name=3D"L0ACF"></a>;; <b>PRINT</b>
L0ACF:  LD      A,(HL)          ;
        CP      $76             ;
        JP      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0B84">L0B84</a>         ; to PRINT-END

<a name=3D"L0AD5"></a>;; <b>PRINT-1</b>
L0AD5:  SUB     $1A             ;
        ADC     A,$00           ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0B44">L0B44</a>         ; to SPACING

        CP      $A7             ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0AFA">L0AFA</a>        ; to NOT-AT


        RST     20H             ; NEXT-CHAR
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0D92">L0D92</a>           ; routine CLASS-6
        CP      $1A             ;
        JP      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0D9A">L0D9A</a>        ; to REPORT-C


        RST     20H             ; NEXT-CHAR
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0D92">L0D92</a>           ; routine CLASS-6
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0B4E">L0B4E</a>           ; routine SYNTAX-ON

        RST     28H             ;; FP-CALC
        DEFB    $01             ;;exchange
        DEFB    $34             ;;end-calc

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0BF5">L0BF5</a>           ; routine STK-TO-BC
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L08F5">L08F5</a>           ; routine PRINT-AT
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0B37">L0B37</a>           ; to PRINT-ON

; ---

<a name=3D"L0AFA"></a>;; <b>NOT-AT</b>
L0AFA:  CP      $A8             ;
        JR      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0B31">L0B31</a>        ; to NOT-TAB


        RST     20H             ; NEXT-CHAR
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0D92">L0D92</a>           ; routine CLASS-6
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0B4E">L0B4E</a>           ; routine SYNTAX-ON
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0C02">L0C02</a>           ; routine STK-TO-A
        JP      NZ,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0EAD">L0EAD</a>        ; to REPORT-B

        AND     $1F             ;
        LD      C,A             ;
        BIT     1,(IY+$01)      ; sv FLAGS  - Is printer in use
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0B1E">L0B1E</a>         ; to TAB-TEST

        SUB     (IY+$38)        ; sv PR_CC
        SET     7,A             ;
        ADD     A,$3C           ;
        CALL    NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0871">L0871</a>        ; routine COPY-BUFF

<a name=3D"L0B1E"></a>;; <b>TAB-TEST</b>
L0B1E:  ADD     A,(IY+$39)      ; sv S_POSN_x
        CP      $21             ;
        LD      A,($403A)       ; sv S_POSN_y
        SBC     A,$01           ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L08FA">L08FA</a>           ; routine TEST-VAL
        SET     0,(IY+$01)      ; sv FLAGS  - Suppress leading space
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0B37">L0B37</a>           ; to PRINT-ON

; ---

<a name=3D"L0B31"></a>;; <b>NOT-TAB</b>
L0B31:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0F55">L0F55</a>           ; routine SCANNING
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0B55">L0B55</a>           ; routine PRINT-STK

<a name=3D"L0B37"></a>;; <b>PRINT-ON</b>
L0B37:  RST     18H             ; GET-CHAR
        SUB     $1A             ;
        ADC     A,$00           ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0B44">L0B44</a>         ; to SPACING

        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0D1D">L0D1D</a>           ; routine CHECK-END
        JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0B84">L0B84</a>           ;;; to PRINT-END

; ---

<a name=3D"L0B44"></a>;; <b>SPACING</b>
L0B44:  CALL    NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0B8B">L0B8B</a>        ; routine FIELD

        RST     20H             ; NEXT-CHAR
        CP      $76             ;
        RET     Z               ;

        JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0AD5">L0AD5</a>           ;;; to PRINT-1

; ---

<a name=3D"L0B4E"></a>;; <b>SYNTAX-ON</b>
L0B4E:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0DA6">L0DA6</a>           ; routine SYNTAX-Z
        RET     NZ              ;

        POP     HL              ;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0B37">L0B37</a>           ; to PRINT-ON

; ---

<a name=3D"L0B55"></a>;; <b>PRINT-STK</b>
L0B55:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0AC5">L0AC5</a>           ; routine UNSTACK-Z
        BIT     6,(IY+$01)      ; sv FLAGS  - Numeric or string result?
        CALL    Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L13F8">L13F8</a>         ; routine STK-FETCH
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0B6B">L0B6B</a>         ; to PR-STR-4

        JP      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L15DB">L15DB</a>           ; jump forward to PRINT-FP

; ---

<a name=3D"L0B64"></a>;; <b>PR-STR-1</b>
L0B64:  LD      A,$0B           ;

<a name=3D"L0B66"></a>;; <b>PR-STR-2</b>
L0B66:  RST     10H             ; PRINT-A

<a name=3D"L0B67"></a>;; <b>PR-STR-3</b>
L0B67:  LD      DE,($4018)      ; sv X_PTR_lo

<a name=3D"L0B6B"></a>;; <b>PR-STR-4</b>
L0B6B:  LD      A,B             ;
        OR      C               ;
        DEC     BC              ;
        RET     Z               ;

        LD      A,(DE)          ;
        INC     DE              ;
        LD      ($4018),DE      ; sv X_PTR_lo
        BIT      6,A            ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0B66">L0B66</a>         ; to PR-STR-2

        CP      $C0             ;
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0B64">L0B64</a>         ; to PR-STR-1

        PUSH    BC              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L094B">L094B</a>           ; routine TOKENS
        POP     BC              ;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0B67">L0B67</a>           ; to PR-STR-3

; ---

<a name=3D"L0B84"></a>;; <b>PRINT-END</b>
L0B84:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0AC5">L0AC5</a>           ; routine UNSTACK-Z
        LD      A,$76           ;

        RST     10H             ; PRINT-A
        RET                     ;

; ---

<a name=3D"L0B8B"></a>;; <b>FIELD</b>
L0B8B:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0AC5">L0AC5</a>           ; routine UNSTACK-Z
        SET     0,(IY+$01)      ; sv FLAGS  - Suppress leading space
        XOR     A               ;

        RST     10H             ; PRINT-A
        LD      BC,($4039)      ; sv S_POSN_x
        LD      A,C             ;
        BIT     1,(IY+$01)      ; sv FLAGS  - Is printer in use
        JR      Z,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0BA4">L0BA4</a>         ; to CENTRE

        LD      A,$5D           ;
        SUB     (IY+$38)        ; sv PR_CC

<a name=3D"L0BA4"></a>;; <b>CENTRE</b>
L0BA4:  LD      C,$11           ;
        CP      C               ;
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0BAB">L0BAB</a>        ; to RIGHT

        LD      C,$01           ;

<a name=3D"L0BAB"></a>;; <b>RIGHT</b>
L0BAB:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L090B">L090B</a>           ; routine SET-FIELD
        RET                     ;

; --------------------------------------
; THE <b><font color=3D"#333388">'PLOT AND UNPLOT'</font></b> COMMAND ROUTI=
NES
; --------------------------------------
;
;

<a name=3D"L0BAF"></a>;; <b>PLOT/UNP</b>
L0BAF:  CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0BF5">L0BF5</a>           ; routine STK-TO-BC
        LD      ($4036),BC      ; sv COORDS_x
        LD      A,$2B           ;
        SUB     B               ;
        JP      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0EAD">L0EAD</a>         ; to REPORT-B

        LD      B,A             ;
        LD      A,$01           ;
        SRA     B               ;
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0BC5">L0BC5</a>        ; to COLUMNS

        LD      A,$04           ;

<a name=3D"L0BC5"></a>;; <b>COLUMNS</b>
L0BC5:  SRA     C               ;
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0BCA">L0BCA</a>        ; to FIND-ADDR

        RLCA                    ;

<a name=3D"L0BCA"></a>;; <b>FIND-ADDR</b>
L0BCA:  PUSH    AF              ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L08F5">L08F5</a>           ; routine PRINT-AT
        LD      A,(HL)          ;
        RLCA                    ;
        CP      $10             ;
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0BDA">L0BDA</a>        ; to TABLE-PTR

        RRCA                    ;
        JR      NC,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0BD9">L0BD9</a>        ; to SQ-SAVED

        XOR     $8F             ;

<a name=3D"L0BD9"></a>;; <b>SQ-SAVED</b>
L0BD9:  LD      B,A             ;

<a name=3D"L0BDA"></a>;; <b>TABLE-PTR</b>
L0BDA:  LD      DE,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx=
81.htm#L0C9E">L0C9E</a>        ; Address: P-UNPLOT
        LD      A,($4030)       ; sv T_ADDR_lo
        SUB     E               ;
        JP      M,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0BE9">L0BE9</a>         ; to PLOT

        POP     AF              ;
        CPL                     ;
        AND     B               ;
        JR      <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L0BEB">L0BEB</a>           ; to UNPLOT

; ---

<a name=3D"L0BE9"></a>;; <b>PLOT</b>
L0BE9:  POP     AF              ;
        OR      B               ;

<a name=3D"L0BEB"></a>;; <b>UNPLOT</b>
L0BEB:  CP      $08             ;
        JR      C,<a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx8=
1.htm#L0BF1">L0BF1</a>         ; to PLOT-END

        XOR     $8F             ;

<a name=3D"L0BF1"></a>;; <b>PLOT-END</b>
L0BF1:  EXX                     ;

        RST     10H             ; PRINT-A
