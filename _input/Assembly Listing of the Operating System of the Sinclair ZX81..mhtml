L1CB1:  DEFB    $A0             ;;stk-zero              <font color=3D"#990=
0FF">Note.</font> not=20
        DEFB    $02             ;;delete                necessary.
        DEFB    $34             ;;end-calc
        LD      A,(HL)          ;

        LD      (HL),$80        ;
        CALL    <a href=3D"https://www.tablix.org/~avian/spectrum/rom/zx81.=
htm#L151D">L151D</a>           ; routine STACK-A

        RST     28H             ;; FP-CALC
