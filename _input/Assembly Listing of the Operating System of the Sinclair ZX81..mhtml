; THE <b><font color=3D"#333388">'CLEAR'</font></b> COMMAND ROUTINE
; ---------------------------
;
;

<a name=3D"L149A"></a>;; <b>CLEAR</b>
L149A:  LD      HL,($4010)      ; sv VARS_lo
