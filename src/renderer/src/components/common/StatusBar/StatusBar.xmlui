<Component name="StatusBar">
  <MachineInfo id="machineInfo" />
  <HStack
    backgroundColor="$backgroundColor-StatusBar"
    color="$colorText-StatusBar"
    height="$height-StatusBar"
    verticalAlignment="center"
    paddingHorizontal="$space-4"
    >
    <StatusBarItem icon="vm" label="Ready"/>
    <SpaceFiller />
    <StatusBarItem label="{console.log(machineInfo), machineInfo.machine?.machine?.displayName ?? 'No machine'}"/>
  </HStack>
</Component>